                if (!String.IsNullOrWhiteSpace(srcEkorg))
                {
                    // 重新rename檔名
                    FileInfo file = new FileInfo(srcRawDataFilePath);
                    string path = Path.GetDirectoryName(srcRawDataFilePath);
                    string renameFile = file.Name.Replace("-" + srcEkorg, "");
