      <DTS:VariableValue
        DTS:DataType="8">select main.SYS_NAME + CASE WHEN main.EKORG &lt;&gt; '' THEN '('+ main.EKORG + ')' ELSE '' END AS SYS_NAME -- 來源系統
	, main.TABLE_NAME      -- 資料表名稱	
	, main.EXECUTIONID
	, CASE WHEN main.FILE_TYPE = 1 THEN 'CHECKFILE' ELSE '資料檔' END AS FILE_TYPE       -- 檔案類型：0=資料檔案,1=checkfile
	, COUNT(1) AS FILE_AMT -- 檔案數量
	 /* ----- 重複下檔統計 ----- */
	 , SUM(CASE WHEN exeEndInfo.FILE_STATUS = 11 THEN 1 ELSE 0 END) AS RELOAD_AMT
	 /* ----- 第一列欄位名稱異常統計 ----- */
	 , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (12) THEN 1 ELSE 0 END) AS COL_ERR_AMT
	 /* ----- 異常統計 ----- */
	 , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (51,61,71) THEN 1 ELSE 0 END) AS ERROR_AMT
	 /* ----- 寫入結果統計 ----- */
	 , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (81,82,83,84) AND ISNULL(veriFailCount.FAIL_AMT,0) = 0 THEN 1 ELSE 0 END) AS SUCCESS_AMT
	 /* ----- 部分資料驗證錯誤無法寫入Stage結果統計 ----- */
	 , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (81,82,83,84) AND ISNULL(veriFailCount.FAIL_AMT,0) &gt; 0 THEN 1 ELSE 0 END) AS VERI_PRESTAGE_FAIL_AMT
	 /* ----- CHECKFILE 驗證結果統計 ----- */
	 , SUM(CASE WHEN FILE_TYPE = 1 THEN 0
				WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE IS NULL THEN 0
				WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = -4 THEN 0
				WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = 4 THEN 1
				ELSE 0 END) AS CHECKFILE_VERI_SUCCESS
	 , SUM(CASE WHEN FILE_TYPE = 1 THEN 0
				WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE IS NULL THEN 0
				WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = -4 THEN 0
				WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = 4 THEN 0
				ELSE 1 END) AS CHECKFILE_VERI_FAIL
	 /* ----- 執行中檔案數量統計 ----- */
	 , SUM(CASE WHEN exeEndInfo.FILE_STATUS &lt; 10 THEN 1 ELSE 0 END) AS EXE_ING_AMT
	 /* ----- 未處理檔案數量統計 ----- */
	 , SUM(CASE WHEN exeEndInfo.FILE_STATUS &gt; 90 THEN 1 ELSE 0 END) AS QUEUE_AMT
	 /* ----- 檔名不符合Pattern數量統計 ----- */
	 , 0 AS PATTERN_ERR_AMT
  from V_FILE_LIST main with(nolock)
  left join V_FILE_HIS_EXE_END exeEndInfo with(nolock)
         on main.FILE_NAME = exeEndInfo.FILE_NAME
	    and main.CHECK_FILE_NAME = exeEndInfo.CHECK_FILE_NAME
		and main.EXECUTIONID = exeEndInfo.EXECUTIONID
  left join V_FILE_VERI_FAIL_COUNT veriFailCount with(nolock)
         on main.FILE_NAME = veriFailCount.FILE_NAME
		and main.EXECUTIONID = veriFailCount.EXECUTIONID
  left join V_FILE_VERI_CHECKFILE veriCheckfile with(nolock)
	     on main.FILE_NAME = veriCheckfile.FILE_NAME
	    and main.CHECK_FILE_NAME = veriCheckfile.CHECK_FILE_NAME
		and main.EXECUTIONID = veriCheckfile.EXECUTIONID
 where main.WORK_DATE = '2022-01-17'  and main.SYS_NAME = 'ECS2' and main.EKORG = ''  and (FILE_TYPE = 1 or (FILE_TYPE = 0 and TABLE_NAME &lt;&gt; ''))  and main.EXECUTIONID =   48688
 group by main.SYS_NAME, main.EKORG, main.TABLE_NAME, main.FILE_TYPE, main.EXECUTIONID
 union
 select SYS_NAME + CASE WHEN EKORG &lt;&gt; '' THEN '('+ EKORG + ')' ELSE '' END AS SYS_NAME -- 來源系統
	, '' AS TABLE_NAME      -- 資料表名稱	
	, EXECUTIONID
	, '不符合ODS檔名規則' AS FILE_TYPE       -- 檔案類型
	, ISNULL(COUNT(1),0) AS FILE_AMT -- 檔案數量
	 /* ----- 重複下檔統計 ----- */
	 , 0 AS RELOAD_AMT
	 /* ----- 第一列欄位名稱異常統計 ----- */
	 , 0 AS COL_ERR_AMT
	 /* ----- 異常統計 ----- */
	 , 0 AS ERROR_AMT
	 /* ----- 寫入結果統計 ----- */
	 , 0 AS SUCCESS_AMT
	 /* ----- 部分資料驗證錯誤無法寫入Stage結果統計 ----- */
	 , 0 AS VERI_PRESTAGE_FAIL_AMT
	 /* ----- CHECKFILE 驗證結果統計 ----- */
	 , 0 AS CHECKFILE_VERI_SUCCESS
	 , 0 AS CHECKFILE_VERI_FAIL
	 /* ----- 執行中檔案數量統計 ----- */
	 , 0 AS EXE_ING_AMT
	 /* ----- 未處理檔案數量統計 ----- */
	 , 0 AS QUEUE_AMT
	 /* ----- 檔名不符合Pattern數量統計 ----- */
	 , ISNULL(COUNT(1),0) AS PATTERN_ERR_AMT
   from FILE_REC
  where WORK_DATE = '2022-01-17'  and SYS_NAME = 'ECS2' and EKORG = ''  and FILE_STATUS in (13)  and EXECUTIONID =   48688
  group by WORK_DATE, SYS_NAME, EKORG, EXECUTIONID</DTS:VariableValue>
