      DTS:Expression="&quot;select main.SYS_NAME + CASE WHEN main.EKORG &lt;&gt; '' THEN '('+ main.EKORG + ')' ELSE '' END AS SYS_NAME -- 來源系統&#xA;&#x9; , main.TABLE_NAME      -- 資料表名稱&#xA;&#x9; , CASE WHEN main.FILE_TYPE = 1 THEN '' WHEN ISNULL(tabs.STATUS,0) = '0' THEN '停用' ELSE '' END AS STATUS&#xA;&#x9; , main.EXECUTIONID     -- 排程執行序號&#xA;&#x9; , main.FILE_NAME       -- 資料檔名&#xA;&#x9; , CASE WHEN main.FILE_TYPE = 1 THEN 'CHECKFILE' ELSE '資料檔' END AS FILE_TYPE       -- 檔案類型：0=資料檔案,1=checkfile&#xA;&#x9; , exeEndInfo.FILE_OP             -- 事件別&#xA;&#x9; , exeEndInfo.FILE_ETL_START_TIME -- 檔案開始ETL作業時間&#xA;&#x9; , round(CAST(DATEDIFF(millisecond,FILE_ETL_START_TIME,FILE_ETL_END_TIME) as float) /1000,2) AS FILE_ETL_DURATION -- 檔案執行ETL作業持續時間(秒)&#xA;&#x9; , (select [DESC] from [StageETLDB].[dbo].[PROJECT_PARAMS] where TYPE = 2 and PARAM_VALUE = exeEndInfo.FILE_STATUS) AS FILE_STATUS -- 檔案最終執行狀態&#xA;&#x9; , CASE WHEN FILE_TYPE = 1 THEN '-'&#xA;&#x9;        WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE IS NULL THEN '-'&#xA;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = -4 THEN '設定不須執行CHECKFILE驗證'&#xA;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = 4 THEN '通過'&#xA;&#x9;&#x9;&#x9;ELSE '失敗' END AS VERI_CHECKFILE -- chekfile驗證結果&#xA;&#x9; , exeEndInfo.ROW_COUNT_REAL      -- 資料筆數&#xA;&#x9; , CASE WHEN main.FILE_TYPE = 1 THEN 0 ELSE ISNULL(veriFailCount.FAIL_AMT,0) END AS FAIL_AMT                                -- 資料驗證失敗筆數&#xA;&#x9; , CASE WHEN main.FILE_TYPE = 1 THEN 0 WHEN exeEndInfo.FILE_STATUS between 10 and 80 then 0 ELSE exeEndInfo.ROW_COUNT_REAL - ISNULL(veriFailCount.FAIL_AMT,0) END AS SUCCESS_AMT -- 資料驗證成功筆數&#xA;&#x9; , main.CHECK_FILE_NAME -- checkfile檔名&#xA;&#x9; , (select top 1 package_name from [ssisdb].[catalog].executables where execution_id = main.EXECUTIONID and '{' + convert(nvarchar(100),package.sourceid) + '}' = executable_guid) AS PACKAGE_NAME&#xA;&#x9; , main.WORK_DATE       -- 批號&#xA;&#x9; , exeEndInfo.CHECK_SUM_REAL      -- checksum&#xA;&#x9; , exeEndInfo.FILE_ETL_END_TIME   -- 檔案結束ETL作業時間&#x9; &#xA;  from (select * from STAGE_TAB_ACT union select * from STAGE_TAB_HIS) tabs&#xA;  full outer join V_FILE_LIST main with(nolock) &#xA;    on tabs.SYS_NAME = main.SYS_NAME &#xA;   and tabs.EKORG = main.EKORG&#xA;   and tabs.TABLE_NAME = main.TABLE_NAME&#xA;   and tabs.WORK_DATE = main.WORK_DATE&#xA;  left join V_FILE_HIS_EXE_END exeEndInfo &#xA;         on main.FILE_NAME = exeEndInfo.FILE_NAME&#xA;&#x9;    and main.CHECK_FILE_NAME = exeEndInfo.CHECK_FILE_NAME&#xA;&#x9;&#x9;and main.EXECUTIONID = exeEndInfo.EXECUTIONID&#xA;  left join V_FILE_VERI_FAIL_COUNT veriFailCount&#xA;         on main.FILE_NAME = veriFailCount.FILE_NAME&#xA;&#x9;&#x9;and main.EXECUTIONID = veriFailCount.EXECUTIONID&#xA;  left join V_FILE_VERI_CHECKFILE veriCheckfile &#xA;&#x9;     on main.FILE_NAME = veriCheckfile.FILE_NAME&#xA;&#x9;    and main.CHECK_FILE_NAME = veriCheckfile.CHECK_FILE_NAME&#xA;&#x9;&#x9;and main.EXECUTIONID = veriCheckfile.EXECUTIONID&#xA;  left join V_FILE_EXE_PACKAGE package on main.FILE_NAME = package.FILE_NAME&#xA; where main.SYS_NAME = '&quot;+ @[$Package::sysName] +&quot;' and main.EKORG = '&quot;+ @[$Package::srcEkorg] +&quot;' and main.WORK_DATE = '&quot;+ @[$Package::workDate] +&quot;'  and (FILE_TYPE = 1 or (FILE_TYPE = 0 and main.TABLE_NAME &lt;&gt; '')) &quot;  + ( @[$Package::executionID] &gt; 0 ?  &quot; and main.EXECUTIONID =   &quot;  + (DT_STR, 100, 950) @[$Package::executionID]   :  &quot;&quot;  )  +  &quot;&#xA; union&#xA; select SYS_NAME + CASE WHEN EKORG &lt;&gt; '' THEN '('+ EKORG + ')' ELSE '' END AS SYS_NAME -- 來源系統&#xA;&#x9; , '' AS TABLE_NAME      -- 資料表名稱&#xA;&#x9; , '' AS STATUS&#xA;&#x9; , EXECUTIONID     -- 排程執行序號&#xA;&#x9; , FILE_NAME       -- 資料檔名&#xA;&#x9; , '不符合ODS檔名規則' AS FILE_TYPE       -- 檔案類型：0=資料檔案,1=checkfile&#xA;&#x9; , '' AS FILE_OP             -- 事件別&#xA;&#x9; , ETL_INSERT_DATE AS FILE_ETL_START_TIME -- 檔案開始ETL作業時間&#xA;&#x9; , round(0,2) AS FILE_ETL_DURATION -- 檔案執行ETL作業持續時間(秒)&#xA;&#x9; , (select [DESC] from [StageETLDB].[dbo].[PROJECT_PARAMS] where TYPE = 2 and PARAM_VALUE = FILE_STATUS) AS FILE_STATUS -- 檔案最終執行狀態&#xA;&#x9; , '-' AS VERI_CHECKFILE -- chekfile驗證結果&#xA;&#x9; , 0 AS ROW_COUNT_REAL      -- 資料筆數&#xA;&#x9; , 0 AS FAIL_AMT                                -- 資料驗證失敗筆數&#xA;&#x9; , 0 AS SUCCESS_AMT -- 資料驗證成功筆數&#xA;&#x9; , ISNULL(CHECK_FILE_NAME,'') AS CHECK_FILE_NAME -- checkfile檔名&#xA;&#x9; , (select top 1 package_name from [ssisdb].[catalog].executables where execution_id = EXECUTIONID and '{' + convert(nvarchar(100),sourceid) + '}' = executable_guid) AS PACKAGE_NAME&#xA;&#x9; , WORK_DATE       -- 批號&#xA;&#x9; , '' AS CHECK_SUM_REAL      -- checksum&#xA;&#x9; , '' AS FILE_ETL_END_TIME   -- 檔案結束ETL作業時間&#xA; from FILE_REC&#xA; where SYS_NAME = '&quot;+ @[$Package::sysName] +&quot;' and EKORG = '&quot;+ @[$Package::srcEkorg] +&quot;' and WORK_DATE = '&quot;+ @[$Package::workDate] +&quot;'  and FILE_STATUS in (13) &quot;  + ( @[$Package::executionID] &gt; 0 ?  &quot; and EXECUTIONID =   &quot;  + (DT_STR, 100, 950) @[$Package::executionID]   :  &quot;&quot;  )  +  &quot;&#xA;order by STATUS ,EXECUTIONID desc, WORK_DATE desc, FILE_NAME&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="sqlFlow2List">
      <DTS:VariableValue
        DTS:DataType="8">select main.SYS_NAME + CASE WHEN main.EKORG &lt;&gt; '' THEN '('+ main.EKORG + ')' ELSE '' END AS SYS_NAME -- 來源系統
	 , main.TABLE_NAME      -- 資料表名稱
	 , CASE WHEN main.FILE_TYPE = 1 THEN '' WHEN ISNULL(tabs.STATUS,0) = '0' THEN '停用' ELSE '' END AS STATUS
	 , main.EXECUTIONID     -- 排程執行序號
	 , main.FILE_NAME       -- 資料檔名
	 , CASE WHEN main.FILE_TYPE = 1 THEN 'CHECKFILE' ELSE '資料檔' END AS FILE_TYPE       -- 檔案類型：0=資料檔案,1=checkfile
	 , exeEndInfo.FILE_OP             -- 事件別
	 , exeEndInfo.FILE_ETL_START_TIME -- 檔案開始ETL作業時間
	 , round(CAST(DATEDIFF(millisecond,FILE_ETL_START_TIME,FILE_ETL_END_TIME) as float) /1000,2) AS FILE_ETL_DURATION -- 檔案執行ETL作業持續時間(秒)
	 , (select [DESC] from [StageETLDB].[dbo].[PROJECT_PARAMS] where TYPE = 2 and PARAM_VALUE = exeEndInfo.FILE_STATUS) AS FILE_STATUS -- 檔案最終執行狀態
	 , CASE WHEN FILE_TYPE = 1 THEN '-'
	        WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE IS NULL THEN '-'
			WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = -4 THEN '設定不須執行CHECKFILE驗證'
			WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = 4 THEN '通過'
			ELSE '失敗' END AS VERI_CHECKFILE -- chekfile驗證結果
	 , exeEndInfo.ROW_COUNT_REAL      -- 資料筆數
	 , CASE WHEN main.FILE_TYPE = 1 THEN 0 ELSE ISNULL(veriFailCount.FAIL_AMT,0) END AS FAIL_AMT                                -- 資料驗證失敗筆數
	 , CASE WHEN main.FILE_TYPE = 1 THEN 0 WHEN exeEndInfo.FILE_STATUS between 10 and 80 then 0 ELSE exeEndInfo.ROW_COUNT_REAL - ISNULL(veriFailCount.FAIL_AMT,0) END AS SUCCESS_AMT -- 資料驗證成功筆數
	 , main.CHECK_FILE_NAME -- checkfile檔名
	 , (select top 1 package_name from [ssisdb].[catalog].executables where execution_id = main.EXECUTIONID and '{' + convert(nvarchar(100),package.sourceid) + '}' = executable_guid) AS PACKAGE_NAME
	 , main.WORK_DATE       -- 批號
	 , exeEndInfo.CHECK_SUM_REAL      -- checksum
	 , exeEndInfo.FILE_ETL_END_TIME   -- 檔案結束ETL作業時間	 
  from (select * from STAGE_TAB_ACT union select * from STAGE_TAB_HIS) tabs
  full outer join V_FILE_LIST main with(nolock) 
    on tabs.SYS_NAME = main.SYS_NAME 
   and tabs.EKORG = main.EKORG
   and tabs.TABLE_NAME = main.TABLE_NAME
   and tabs.WORK_DATE = main.WORK_DATE
  left join V_FILE_HIS_EXE_END exeEndInfo 
         on main.FILE_NAME = exeEndInfo.FILE_NAME
	    and main.CHECK_FILE_NAME = exeEndInfo.CHECK_FILE_NAME
		and main.EXECUTIONID = exeEndInfo.EXECUTIONID
  left join V_FILE_VERI_FAIL_COUNT veriFailCount
         on main.FILE_NAME = veriFailCount.FILE_NAME
		and main.EXECUTIONID = veriFailCount.EXECUTIONID
  left join V_FILE_VERI_CHECKFILE veriCheckfile 
	     on main.FILE_NAME = veriCheckfile.FILE_NAME
	    and main.CHECK_FILE_NAME = veriCheckfile.CHECK_FILE_NAME
		and main.EXECUTIONID = veriCheckfile.EXECUTIONID
  left join V_FILE_EXE_PACKAGE package on main.FILE_NAME = package.FILE_NAME
 where main.SYS_NAME = 'ECS2' and main.EKORG = '' and main.WORK_DATE = '2022-01-17'  and (FILE_TYPE = 1 or (FILE_TYPE = 0 and main.TABLE_NAME &lt;&gt; ''))  and main.EXECUTIONID =   48688
 union
 select SYS_NAME + CASE WHEN EKORG &lt;&gt; '' THEN '('+ EKORG + ')' ELSE '' END AS SYS_NAME -- 來源系統
	 , '' AS TABLE_NAME      -- 資料表名稱
	 , '' AS STATUS
	 , EXECUTIONID     -- 排程執行序號
	 , FILE_NAME       -- 資料檔名
	 , '不符合ODS檔名規則' AS FILE_TYPE       -- 檔案類型：0=資料檔案,1=checkfile
	 , '' AS FILE_OP             -- 事件別
	 , ETL_INSERT_DATE AS FILE_ETL_START_TIME -- 檔案開始ETL作業時間
	 , round(0,2) AS FILE_ETL_DURATION -- 檔案執行ETL作業持續時間(秒)
	 , (select [DESC] from [StageETLDB].[dbo].[PROJECT_PARAMS] where TYPE = 2 and PARAM_VALUE = FILE_STATUS) AS FILE_STATUS -- 檔案最終執行狀態
	 , '-' AS VERI_CHECKFILE -- chekfile驗證結果
	 , 0 AS ROW_COUNT_REAL      -- 資料筆數
	 , 0 AS FAIL_AMT                                -- 資料驗證失敗筆數
	 , 0 AS SUCCESS_AMT -- 資料驗證成功筆數
	 , ISNULL(CHECK_FILE_NAME,'') AS CHECK_FILE_NAME -- checkfile檔名
	 , (select top 1 package_name from [ssisdb].[catalog].executables where execution_id = EXECUTIONID and '{' + convert(nvarchar(100),sourceid) + '}' = executable_guid) AS PACKAGE_NAME
	 , WORK_DATE       -- 批號
	 , '' AS CHECK_SUM_REAL      -- checksum
	 , '' AS FILE_ETL_END_TIME   -- 檔案結束ETL作業時間
 from FILE_REC
 where SYS_NAME = 'ECS2' and EKORG = '' and WORK_DATE = '2022-01-17'  and FILE_STATUS in (13)  and EXECUTIONID =   48688
order by STATUS ,EXECUTIONID desc, WORK_DATE desc, FILE_NAME</DTS:VariableValue>
    </DTS:Variable>
    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{5E4C5A6A-4A38-450B-B04E-3FCF9B36085A}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT F.TABLE_NAME &#xA;  FROM STAGINGDB_TW.DBO.FILE_REC F &#xA;  JOIN STAGINGDB_TW.DBO.STAGE_TAB_CTL C&#xA;    ON F.SYS_NAME = C.SYS_NAME&#xA;   AND F.EKORG = C.EKORG&#xA;   AND F.TABLE_NAME = C.TABLE_NAME&#xA;   AND C.MAIL_TO_SYSADMIN = 1&#xA;  where F.SYS_NAME = '&quot;+@[$Package::sysName]+&quot;'&#xA;    and F.WORK_DATE = '&quot;+@[$Package::workDate]+&quot;'&#xA;    and F.EKORG = '&quot;+@[$Package::srcEkorg]+&quot;'&#xA;&#x9;AND F.FILE_TYPE != 1&#xA;  GROUP BY F.TABLE_NAME&#xA;  HAVING SUM(CASE WHEN FILE_TYPE = 2 THEN -1&#xA;&#x9;&#x9;&#x9;&#x9;  WHEN FILE_STATUS = 99 AND FILE_OP IS NULL THEN -1 &#xA;&#x9;&#x9;&#x9;      ELSE 1 END) &lt; 0&quot;"
      DTS:IncludeInDebugDump="2345"
