        DTS:DataType="8">SELECT F.TABLE_NAME 
  FROM STAGINGDB_TW.DBO.FILE_REC F 
  JOIN STAGINGDB_TW.DBO.STAGE_TAB_CTL C
    ON F.SYS_NAME = C.SYS_NAME
   AND F.EKORG = C.EKORG
   AND F.TABLE_NAME = C.TABLE_NAME
   AND C.MAIL_TO_SYSADMIN = 1
  where F.SYS_NAME = 'ECS2'
    and F.WORK_DATE = '2022-01-17'
    and F.EKORG = ''
	AND F.FILE_TYPE != 1
  GROUP BY F.TABLE_NAME
  HAVING SUM(CASE WHEN FILE_TYPE = 2 THEN -1
