    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{A903F9F2-B877-47F5-8606-010C61094E86}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;select top 5000 main.SYS_NAME + CASE WHEN main.EKORG &lt;&gt; '' THEN '('+ main.EKORG + ')' ELSE '' END AS SYS_NAME -- 來源系統&#xA;    , main.EXECUTIONID&#xA;    , main.FILE_NAME&#xA;&#x9;, (select [DESC] from  StageETLDB.dbo.PROJECT_PARAMS where PARAM_VALUE = VPI.VERI_PROC_PKEY and TYPE = 1) AS VERI_DESC&#xA;&#x9;, VPI.COLUMN_NAME&#xA;&#x9;, SUBSTRING(VPLFD.RAW_DATA_PK,1,255) AS RAW_DATA_PK&#xA;&#x9;, SUBSTRING(VPLFD.RAW_DATA,1,255) AS RAW_DATA&#xA;&#x9;, VPL.START_VERI_DATE&#xA;&#x9;, VPL.END_VERI_DATE&#xA;from V_FILE_LIST main with(nolock) &#xA;join dbo.VERI_PRESTAGE_LOG VPL on main.FILE_NAME = VPL.FILE_NAME and main.EXECUTIONID = VPL.EXECUTIONID&#xA;join dbo.VERI_PRESTAGE_ITEM VPI on VPL.ITEM_PKEY = VPI.PKEY&#xA;join dbo.VERI_PRESTAGE_LOG_FAIL_DETAIL VPLFD on VPL.PKEY = VPLFD.LOG_PKEY&#xA;where (select [DESC] from  StageETLDB.dbo.PROJECT_PARAMS where PARAM_VALUE = VPI.VERI_PROC_PKEY and TYPE = 1) != '驗證D檔刪除的PKEY是否重複' and main.SYS_NAME = '&quot;+ @[$Package::sysName] +&quot;' and main.EKORG = '&quot;+ @[$Package::srcEkorg] +&quot;' and main.WORK_DATE = '&quot;+ @[$Package::workDate]   + ( @[$Package::executionID] &gt; 0 ?  &quot;' and main.EXECUTIONID =   &quot;  + (DT_STR, 100, 950) @[$Package::executionID]   :  &quot;'&quot;  )   +&quot;   &#xA;order by ROW_NUMBER() OVER(order by main.EXECUTIONID desc, WORK_DATE desc, main.FILE_NAME, VPI.VERI_PROC_PKEY, VPLFD.PKEY)&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="sqlDetailList">
      <DTS:VariableValue
        DTS:DataType="8">select top 5000 main.SYS_NAME + CASE WHEN main.EKORG &lt;&gt; '' THEN '('+ main.EKORG + ')' ELSE '' END AS SYS_NAME -- 來源系統
    , main.EXECUTIONID
    , main.FILE_NAME
	, (select [DESC] from  StageETLDB.dbo.PROJECT_PARAMS where PARAM_VALUE = VPI.VERI_PROC_PKEY and TYPE = 1) AS VERI_DESC
	, VPI.COLUMN_NAME
	, SUBSTRING(VPLFD.RAW_DATA_PK,1,255) AS RAW_DATA_PK
	, SUBSTRING(VPLFD.RAW_DATA,1,255) AS RAW_DATA
	, VPL.START_VERI_DATE
	, VPL.END_VERI_DATE
from V_FILE_LIST main with(nolock) 
join dbo.VERI_PRESTAGE_LOG VPL on main.FILE_NAME = VPL.FILE_NAME and main.EXECUTIONID = VPL.EXECUTIONID
join dbo.VERI_PRESTAGE_ITEM VPI on VPL.ITEM_PKEY = VPI.PKEY
join dbo.VERI_PRESTAGE_LOG_FAIL_DETAIL VPLFD on VPL.PKEY = VPLFD.LOG_PKEY
where (select [DESC] from  StageETLDB.dbo.PROJECT_PARAMS where PARAM_VALUE = VPI.VERI_PROC_PKEY and TYPE = 1) != '驗證D檔刪除的PKEY是否重複' and main.SYS_NAME = 'ECS2' and main.EKORG = '' and main.WORK_DATE = '2022-01-17' and main.EXECUTIONID =   48688   
order by ROW_NUMBER() OVER(order by main.EXECUTIONID desc, WORK_DATE desc, main.FILE_NAME, VPI.VERI_PROC_PKEY, VPLFD.PKEY)
</DTS:VariableValue>
    </DTS:Variable>
    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{92D6A1C3-0976-4934-811B-E527C7BF39B3}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;select main.SYS_NAME + CASE WHEN main.EKORG &lt;&gt; '' THEN '('+ main.EKORG + ')' ELSE '' END AS SYS_NAME -- 來源系統&#xA;&#x9;, main.TABLE_NAME      -- 資料表名稱&#x9;&#xA;&#x9;, main.EXECUTIONID&#xA;&#x9;, CASE WHEN main.FILE_TYPE = 1 THEN 'CHECKFILE' ELSE '資料檔' END AS FILE_TYPE       -- 檔案類型：0=資料檔案,1=checkfile&#xA;&#x9;, COUNT(1) AS FILE_AMT -- 檔案數量&#xA;&#x9; /* ----- 重複下檔統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS = 11 THEN 1 ELSE 0 END) AS RELOAD_AMT&#xA;&#x9; /* ----- 第一列欄位名稱異常統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (12) THEN 1 ELSE 0 END) AS COL_ERR_AMT&#xA;&#x9; /* ----- 異常統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (51,61,71) THEN 1 ELSE 0 END) AS ERROR_AMT&#xA;&#x9; /* ----- 寫入結果統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (81,82,83,84) AND ISNULL(veriFailCount.FAIL_AMT,0) = 0 THEN 1 ELSE 0 END) AS SUCCESS_AMT&#xA;&#x9; /* ----- 部分資料驗證錯誤無法寫入Stage結果統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (81,82,83,84) AND ISNULL(veriFailCount.FAIL_AMT,0) &gt; 0 THEN 1 ELSE 0 END) AS VERI_PRESTAGE_FAIL_AMT&#xA;&#x9; /* ----- CHECKFILE 驗證結果統計 ----- */&#xA;&#x9; , SUM(CASE WHEN FILE_TYPE = 1 THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE IS NULL THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = -4 THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = 4 THEN 1&#xA;&#x9;&#x9;&#x9;&#x9;ELSE 0 END) AS CHECKFILE_VERI_SUCCESS&#xA;&#x9; , SUM(CASE WHEN FILE_TYPE = 1 THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE IS NULL THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = -4 THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = 4 THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;ELSE 1 END) AS CHECKFILE_VERI_FAIL&#xA;&#x9; /* ----- 執行中檔案數量統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndI