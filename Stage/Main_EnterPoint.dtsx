            <DTS:Executable
              DTS:refId="Package\主ETL作業區塊\持續掃描指定資料夾\取得每次掃描得到的所有檔案的資料清洗結果"
              DTS:CreationName="Microsoft.ExecuteSQLTask"
              DTS:Description="執行 SQL 工作"
              DTS:DTSID="{6B226CC0-70EF-4769-A6F7-9F2EE0F42F7A}"
              DTS:ExecutableType="Microsoft.ExecuteSQLTask"
              DTS:LocaleID="-1"
              DTS:ObjectName="取得每次掃描得到的所有檔案的資料清洗結果"
              DTS:ThreadHint="1">
              <DTS:Variables />
              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{317F7F78-B848-480C-B983-2137EF141394}"
                  SQLTask:SqlStatementSource="select ISNULL(SUM(RELOAD_AMT) + SUM(ERROR_AMT) + SUM(CHECKFILE_VERI_FAIL) + SUM(VERI_PRESTAGE_FAIL_AMT),0) AS ERR_AMT&#xA;  from (select main.SYS_NAME + CASE WHEN main.EKORG &lt;&gt; '' THEN '('+ main.EKORG + ')' ELSE '' END AS SYS_NAME -- 來源系統&#xA;&#xA; , main.TABLE_NAME      -- 資料表名稱&#xA;&#x9;, CASE WHEN main.FILE_TYPE = 1 THEN 'CHECKFILE' ELSE '資料檔' END AS FILE_TYPE       -- 檔案類型：0=資料檔案,1=checkfile&#xA;&#x9; , main.EXECUTIONID&#xA;, COUNT(1) AS FILE_AMT -- 檔案數量&#xA;&#x9; /* ----- CHECKFILE 驗證結果統計 ----- */&#xA;&#x9; , SUM(CASE WHEN FILE_TYPE = 1 THEN 1&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE IS NULL THEN 1&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = -4 THEN 1&#xA;&#x9;&#x9;&#x9;&#x9;ELSE 0 END) AS CHECKFILE_VERI_NONE&#xA;&#x9; , SUM(CASE WHEN FILE_TYPE = 1 THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE IS NULL THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = -4 THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = 4 THEN 1&#xA;&#x9;&#x9;&#x9;&#x9;ELSE 0 END) AS CHECKFILE_VERI_SUCCESS&#xA;&#x9; , SUM(CASE WHEN FILE_TYPE = 1 THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE IS NULL THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = -4 THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;WHEN FILE_TYPE = 0 and veriCheckfile.VERI_CHECKFILE = 4 THEN 0&#xA;&#x9;&#x9;&#x9;&#x9;ELSE 1 END) AS CHECKFILE_VERI_FAIL&#xA;&#x9; /* ----- 重複下檔統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS = 11 THEN 1 ELSE 0 END) AS RELOAD_AMT&#xA;&#x9; /* ----- 異常統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (12,51,61,71) THEN 1 ELSE 0 END) AS ERROR_AMT&#xA;&#x9; /* ----- 寫入結果統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (81,82,83,84) AND ISNULL(veriFailCount.FAIL_AMT,0) = 0 THEN 1 ELSE 0 END) AS SUCCESS_AMT&#xA;&#x9; /* ----- 部分資料驗證錯誤無法寫入Stage結果統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS in (81) AND ISNULL(veriFailCount.FAIL_AMT,0) &gt; 0 THEN 1 ELSE 0 END) AS VERI_PRESTAGE_FAIL_AMT&#xA;&#x9; /* ----- 執行中檔案數量統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS &lt; 10 THEN 1 ELSE 0 END) AS EXE_ING_AMT&#xA;&#x9; /* ----- 未處理檔案數量統計 ----- */&#xA;&#x9; , SUM(CASE WHEN exeEndInfo.FILE_STATUS &gt; 90 THEN 1 ELSE 0 END) AS QUEUE_AMT&#xA;  from V_FILE_LIST main with(nolock)&#xA;  left join V_FILE_HIS_EXE_END exeEndInfo with(nolock)&#xA;         on main.FILE_NAME = exeEndInfo.FILE_NAME&#xA;&#x9;    and main.CHECK_FILE_NAME = exeEndInfo.CHECK_FILE_NAME&#xA;&#x9;&#x9;and main.EXECUTIONID = exeEndInfo.EXECUTIONID&#xA;  left join V_FILE_VERI_FAIL_COUNT veriFailCount with(nolock)&#xA;         on main.FILE_NAME = veriFailCount.FILE_NAME&#xA;&#x9;&#x9;and main.EXECUTIONID = veriFailCount.EXECUTIONID&#xA;  left join V_FILE_VERI_CHECKFILE veriCheckfile with(nolock)&#xA;&#x9;     on main.FILE_NAME = veriCheckfile.FILE_NAME&#xA;&#x9;    and main.CHECK_FILE_NAME = veriCheckfile.CHECK_FILE_NAME&#xA;&#x9;&#x9;and main.EXECUTIONID = veriCheckfile.EXECUTIONID&#xA; where main.WORK_DATE = ?  and main.SYS_NAME = ? and main.EKORG = ?  and (FILE_TYPE = 1 or (FILE_TYPE = 0 and TABLE_NAME &lt;&gt; ''))  and main.EXECUTIONID = ?&#xA; group by main.SYS_NAME, main.EKORG, main.TABLE_NAME, main.FILE_TYPE, main.EXECUTIONID&#xA; )tmp"
                  SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
                  <SQLTask:ResultBinding
                    SQLTask:ResultName="0"
                    SQLTask:DtsVariableName="User::errNo" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="0"
                    SQLTask:DtsVariableName="User::param_WorkDate"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="129"
                    SQLTask:ParameterSize="-1" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="1"
                    SQLTask:DtsVariableName="$Package::srcSysName"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="129"
                    SQLTask:ParameterSize="-1" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="2"
                    SQLTask:DtsVariableName="$Package::srcEkorg"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="129"
                    SQLTask:ParameterSize="-1" />
                  <SQLTask:ParameterBinding
                    SQLTask:ParameterName="3"
                    SQLTask:DtsVariableName="System::ServerExecutionID"
                    SQLTask:ParameterDirection="Input"
                    SQLTask:DataType="3"
                    SQLTask:ParameterSize="-1" />
                </SQLTask:SqlTaskData>
              </DTS:ObjectData>
            </DTS:Executable>
