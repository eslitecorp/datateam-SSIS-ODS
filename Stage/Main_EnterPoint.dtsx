		public void Main()
		{
            HashSet<String> fileNotMatchedHashSet = (HashSet<String>)Dts.Variables["User::fileNotMatchedHashSet"].Value;
            //string fileNotMatchedVal = Dts.Variables["User::fileNotMatchedVal"].Value.ToString();
            string patternNotMatchedFileFolder = Dts.Variables["User::patternNotMatchedFileFolder"].Value.ToString();

            DataTable dt = new DataTable();
            dt.Columns.Add("fileName");
            foreach (string fileNotMatchedVal in fileNotMatchedHashSet)
            {
                if (File.Exists(fileNotMatchedVal))
                {
                    string fileName = Path.GetFileName(fileNotMatchedVal);
                    File.Move(fileNotMatchedVal, patternNotMatchedFileFolder + fileName);

                    DataRow dr = dt.NewRow();
                    dr["fileName"] = fileName;
                    dt.Rows.Add(dr);
                }
            }
            Dts.Variables["User::fileNotMatchedDataTable"].Value = dt;
