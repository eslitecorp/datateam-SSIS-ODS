         * */
        #endregion


		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            try
            {
                // 1.宣告所需變數
                const string archive = "archive";
                OleDbDataAdapter adapter = new OleDbDataAdapter();
                DataTable filePathDT = new DataTable();                
                adapter.Fill(filePathDT, Dts.Variables["User::filePathResultSet"].Value);
                DataTable needClearFileDT = new DataTable();
                needClearFileDT.Columns.Add("fileName");
                needClearFileDT.Columns.Add("archiveFolder");
                needClearFileDT.Columns.Add("lastWriteTime");
                needClearFileDT.Columns.Add("fileNameWithoutExt");

                string folderNamesStr = Dts.Variables["User::folderNames"].Value.ToString();
                string[] folderNames = folderNamesStr.Split(';');
                StringBuilder msg = new StringBuilder();

                // 2.建立需要處理檔案之dataTable
                foreach (DataRow row in filePathDT.Rows)
                {
                    string fp = row[0].ToString();  // 此系統之 ETL FOLDER_PATH
                    string archiveFolder = fp + archive + "\\";
                    if (Directory.Exists(archiveFolder))
                    {
                        foreach(string fn in folderNames)
                        {
                            string archiveDestFolder = archiveFolder + fn + "\\";
                            if (!Directory.Exists(archiveDestFolder))
                            {
                                continue;
                            }
                            String[] fileNames = Directory.GetFiles(archiveDestFolder);
                            foreach (string fullFilePath in fileNames)
                            {
                                FileInfo f = new FileInfo(fullFilePath);
                                DataRow dr = needClearFileDT.NewRow();

                                // 去除副檔名與後置詞
                                string fileNameWithoutExt = Path.GetFileNameWithoutExtension(f.Name);
                                dr["fileName"] = f.Name;
                                dr["archiveFolder"] = archiveDestFolder;
                                dr["lastWriteTime"] = f.LastWriteTime;
                                dr["fileNameWithoutExt"] = fileNameWithoutExt;
                                needClearFileDT.Rows.Add(dr);
                            }
                        }
                    }
                }
                Dts.Variables["User::mail_messageSource"].Value = msg.ToString();
                Dts.Variables["User::needClearFileDT"].Value = needClearFileDT;
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception)
            {
                Dts.TaskResult = (int)ScriptResults.Failure;
            }

		}

