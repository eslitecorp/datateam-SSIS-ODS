         *  Dts.Connections["Sales DB"].ReleaseConnection(rawConnection);
         *
         * Example of using a File connection manager
         *  object rawConnection = Dts.Connections["Prices.zip"].AcquireConnection(Dts.Transaction);
         *  string filePath = (string)rawConnection;
         *  //Use the connection in some code here, then release the connection
         *  Dts.Connections["Prices.zip"].ReleaseConnection(rawConnection);
         * */
        #endregion


		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            try
            {
                // 1.宣告所需變數
                const string archive = "archive";
                OleDbDataAdapter adapter = new OleDbDataAdapter();
                DataTable filePathDT = new DataTable();
                adapter.Fill(filePathDT, Dts.Variables["User::filePathResultSet"].Value);
                DataTable needZipFileDT = new DataTable();
                needZipFileDT.Columns.Add("fileName");
                needZipFileDT.Columns.Add("archiveFolder");
                String folderNames = Dts.Variables["$Package::folderNames"].Value.ToString();
                String[] folders = folderNames.Split(';');  // 需處理之資料夾清單(done、error、patternNotMatched)
                StringBuilder msg = new StringBuilder();

                // 2.處理各系統資料夾
                foreach (DataRow row in filePathDT.Rows)
                {
                    string fp = row[0].ToString();                  // 此系統之 ETL FOLDER_PATH
                    string archiveFolder = fp + archive + "\\";     // archive資料夾下建立done、error、patternNotMatched                    
                    if (!Directory.Exists(fp))
                    {
                        continue;
                    }
                    if (!Directory.Exists(archiveFolder))
                    {
                        Directory.CreateDirectory(archiveFolder);
                    }

                    foreach (string f in folders)
                    {
                        string sfp = fp + f + "\\";                     // 要處理之資料夾(done、error、patternNotMatched )
                        if (Directory.Exists(sfp))
                        {
                            string archiveDestFolder = archiveFolder + f + "\\";    // archive下對應處理之資料夾
                            if (!Directory.Exists(archiveDestFolder))
                            {
                                Directory.CreateDirectory(archiveDestFolder);
                            }
                            
                            String[] fileNames = Directory.GetFiles(sfp);
                            foreach (string fullFilePath in fileNames)
                            {
                                string fileName = Path.GetFileName(fullFilePath);
                                string ext = Path.GetExtension(fullFilePath);
                                string preName = Path.GetFileNameWithoutExtension(fileName);
                                if (!(ext.Contains("zip") || ext.Contains("7z") || ext.Contains("rar")))
                                {
                                    // 非壓縮檔先移致archive
                                    DataRow dr = needZipFileDT.NewRow();
                                    dr["fileName"] = fileName;
                                    dr["archiveFolder"] = archiveDestFolder;
                                    needZipFileDT.Rows.Add(dr);
                                    ForceMove(sfp + fileName, archiveDestFolder + fileName, msg);
                                }
                                else
                                {
                                    // 壓縮檔移至archive下對應資料夾
                                    ForceMove(sfp + fileName, archiveDestFolder + fileName, msg);
                                    File.SetLastWriteTime(archiveDestFolder + fileName, DateTime.Now);
                                }
                            }
                        }
                    }
                }

                Dts.Variables["User::mail_messageSource"].Value = msg.ToString();
                Dts.Variables["User::needZipFileDT"].Value = needZipFileDT;
                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception)
            {
                Dts.TaskResult = (int)ScriptResults.Failure;
            }

		}

        // 強制移動檔案 (如原來目的地已有檔案先刪除)
        public void ForceMove(string source, string dest, StringBuilder msg)
        {
            if(File.Exists(dest))
            {
                File.Delete(dest);
            }
            File.Move(source, dest);
            msg.AppendLine(source + ";" + dest);
        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
