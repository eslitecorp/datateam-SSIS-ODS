		public void Main()
		{
            string ekorg = Dts.Variables["$Package::srcEkorg"].Value.ToString();
            string srcRawDataFileName = Dts.Variables["$Package::srcCheckFileName"].Value.ToString();
            string srcRawDataFilePath = Dts.Variables["$Package::srcCheckFilePath"].Value.ToString();
                        
            if(!String.IsNullOrWhiteSpace(ekorg))
            {
                FileInfo file = new FileInfo(srcRawDataFileName);
                string ext = file.Extension;
                string preName = Path.GetFileNameWithoutExtension(file.Name);
                string path = Path.GetDirectoryName(srcRawDataFilePath);
                string renameFile = preName + "-" + ekorg + ext;
                File.Move(path + "\\" + file.Name, path + "\\" + renameFile);

                Dts.Variables["User::reSrcCheckFileName"].Value = renameFile;
                Dts.Variables["User::reSrcCheckFilePath"].Value = path + "\\" + renameFile;
            } else
            {
                Dts.Variables["User::reSrcCheckFileName"].Value = srcRawDataFileName;
                Dts.Variables["User::reSrcCheckFilePath"].Value = srcRawDataFilePath;
            }

