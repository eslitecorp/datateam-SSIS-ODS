RANS_TYPE&#xA;&#x9; , R_SUB.CREATED_AT AS TRANS_DATE&#xA;&#x9; , R_SUB.ESP_INVOICE_NUMBER_REVOKED_DATE AS INVOICE_DATE&#xA;&#x9; , MAIN.CREATED_AT AS ORDER_DATE&#xA;&#x9; , MAIN.INVOICE_TYPE AS INV_TYPE&#xA;&#x9; , CASE WHEN MAIN.INVOICE_NUMBER = ''&#xA;&#x9;        THEN SUB.ESP_INVOICE_NUMBER&#xA;&#x9;&#x9;&#x9;ELSE MAIN.INVOICE_NUMBER END AS INVOICE_NO&#xA;&#x9; , R_SUB.R_SALES_AMT AS INVOICE_AMT&#xA;&#x9; , R_SUB.R_SALES_AMT + R_SUB.ESLITE_POINT AS SALES_AMT&#xA;&#x9; , R_SUB.R_NOTAX_AMT AS SALES_AMT_NOTAX &#xA;&#x9; , R_SUB.R_DISC_AMT AS TOT_DISC_AMT&#xA;&#x9; , R_SUB.R_SALES_AMT + R_SUB.ESLITE_POINT AS SL_EC_AMT&#xA;&#x9; , CARD.CARDCODE AS CARD_ID&#xA;&#x9; , CARD.CARDNUMBER AS MEMBER_ID&#xA;&#x9; , SUB.SP19_CARD_LEVEL AS MEMBER_EN&#xA;&#x9; , SUB.SP19_PROMOTION_CARD_LEVEL AS MEMBER_EN_PARENT&#xA;&#x9; , '' AS SHIP_TYPE&#xA;&#x9; , R_SUB.CREATED_AT AS RTN_DATE&#xA;&#x9; , SUB.ESP_INVOICE_NUMBER_CREATED_DATE AS RTN_BILLING_DATE&#xA;&#x9; , ACC.ACCOUNT AS EC_CUST&#xA;&#x9; , CONCAT('P',R_SUB.SUB_ORDER_ID) AS SL_KEY_O&#xA;&#x9; , '-1' AS FLAG&#xA;&#x9; , R_SUB.R_TAX_AMT AS TAX&#xA;&#x9; , MAIN.TAXID AS BUYER_ID&#xA;&#x9; , MAIN.ORDER_NUMBER AS ORDER_NUMBER&#xA;&#x9; , CASE SUB.IS_B2B_ORDER WHEN 1 THEN 'Y'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  WHEN 0 THEN 'N'&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;  ELSE '' END AS IS_B2B_ORDER&#xA;&#x9; , '' AS RECIPIENT_STORE_ID&#xA;&#x9; , '' AS RECIPIENT_STORE_NAME&#xA;&#x9; , '' AS RECIPIENT_STORE_TYPE&#xA;&#x9; , '' AS RECIPIENT_COUNTRY&#xA;&#x9; , '' AS RECIPIENT_ZIP&#xA;&#x9; , '' AS RECIPIENT_DISTRICT&#xA;&#x9; , '' AS RECIPIENT_ADDRESS&#xA;&#x9; , R_SUB.RETURN_TYPE AS RETURN_TYPE&#xA;&#x9; , R_SUB.ETL_DATE&#xA;&#x9; , R_SUB.FOR_GROUP&#xA;&#x9; , '' AS RECIPIENT_CITY&#xA;&#x9; , R_SUB.ESLITE_POINT AS DSC_POINT_AMT&#xA;&#x9; , MAIN.ORDER_SOURCE &#xA;     , NULL AS ARRIVAL_DATE&#xA;     , NULL AS PICKUP_DATE&#xA;FROM (SELECT R_SUB.MAIN_ORDER_ID , R_SUB.SUB_ORDER_ID , CONVERT(VARCHAR(10) , R_SUB.CREATED_AT ,111) AS FOR_GROUP , MAX(R_SUB.CREATED_AT) AS CREATED_AT, R_SUB.RETURN_TYPE , MAX(R_SUB.ETL_DATE) AS ETL_DATE , MIN(R_SUB.ID) AS MIN_ID , SUM(R_SUB.PRICE) AS R_SALES_AMT , SUM(ISNULL(R_SUB.TAX,0)) AS R_TAX_AMT ,&#xA;             SUM(ISNULL(R_SUB.UNTAXAMOUNT,0)) AS R_NOTAX_AMT , SUM(ISNULL(SUB.DISCOUNT_AMOUNT_PRICE,0)) AS R_DISC_AMT , MIN(ESP_INVOICE_NUMBER_REVOKED_DATE) AS ESP_INVOICE_NUMBER_REVOKED_DATE , SUM(ESLITE_POINT) AS ESLITE_POINT&#xA;&#x9;&#x9;&#x9; &#xA;&#x9;&#x9;FROM ECS2.RETURN_SUB_ORDER_ITEMS R_SUB&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;LEFT JOIN ECS2.SUB_ORDER_ITEMS SUB&#xA;&#x9;&#x9;  ON R_SUB.MAIN_ORDER_ID = SUB.MAIN_ORDER_ID&#xA;&#x9;&#x9; AND R_SUB.SUB_ORDER_ID = SUB.SUB_ORDER_ID&#xA;&#x9;&#x9; AND R_SUB.SUB_ORDER_ITEM_ID = SUB.ID&#xA;&#x9;&#x9; &#xA;&#x9;   GROUP BY R_SUB.MAIN_ORDER_ID , R_SUB.SUB_ORDER_ID , CONVERT(VARCHAR(10) , R_SUB.CREATED_AT ,111) , R_SUB.RETURN_TYPE ) R_SUB&#xA;&#x9;   &#xA;JOIN ECS2.SUB_ORDERS SUB&#xA;  ON R_SUB.MAIN_ORDER_ID = SUB.MAIN_ORDER_ID&#xA; AND R_SUB.SUB_ORDER_ID = SUB.ID&#xA; &#xA;JOIN ECS2.MAIN_ORDERS MAIN&#xA;  ON R_SUB.MAIN_ORDER_ID = MAIN.ID&#xA;  &#xA;OUTER APPLY (SELECT TOP 1 * FROM EC.MEMBER_ESLITECARDINF CARD&#xA;&#x9;&#x9;&#x9;  WHERE MAIN.MEMBER_UUID = CARD.USERID &#xA;&#x9;&#x9;&#x9;    AND CARD.DELETE_MARK IS NULL&#xA;&#x9;&#x9;&#x9;  ORDER BY LASTMODIFYDATE DESC ) CARD&#xA;  &#xA;LEFT JOIN EC.ACCOUNT_DATA ACC&#xA;  ON MAIN.MEMBER_UUID = ACC.ACCOUNT_SEQNO&#xA;  &#xA;WHERE R_SUB.ETL_DATE &gt;= cast(cast('&quot; + ((@[$Package::queryBaseDate]==&quot;&quot;) ? (DT_STR, 50, 950) GETDATE()  : @[$Package::queryBaseDate]) + &quot;' as date) as datetime)&#xA;   OR SUB.ETL_DATE &gt;= cast(cast('&quot; + ((@[$Package::queryBaseDate]==&quot;&quot;) ? (DT_STR, 50, 950) GETDATE()  : @[$Package::queryBaseDate]) + &quot;' as date) as datetime)&#xA;   OR MAIN.ETL_DATE &gt;= cast(cast('&quot; + ((@[$Package::queryBaseDate]==&quot;&quot;) ? (DT_STR, 50, 950) GETDATE()  : @[$Package::queryBaseDate]) + &quot;' as date) as datetime)&#xA;   OR CARD.ETL_DATE &gt;= cast(cast('&quot; + ((@[$Package::queryBaseDate]==&quot;&quot;) ? (DT_STR, 50, 950) GETDATE()  : @[$Package::queryBaseDate]) + &quot;' as date) as datetime)&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="queryFromStage">
      <DTS:VariableValue
        DTS:DataType="8">SELECT CONCAT('P',SUB.ID) AS SL_KEY
     , SUB.ORDER_NUMBER AS TRANS_SEQNO
	 , '1' AS TRANS_TYPE
	 , SUB.ESP_INVOICE_NUMBER_CREATED_DATE AS TRANS_DATE
	 , ISNULL(SUB.ESP_INVOICE_NUMBER_CREATED_DATE,MAIN.INVOICE_DATE) AS INVOICE_DATE
	 , MAIN.CREATED_AT AS ORDER_DATE
	 , MAIN.INVOICE_TYPE AS INV_TYPE
	 , CASE WHEN MAIN.INVOICE_NUMBER = ''
	        THEN SUB.ESP_INVOICE_NUMBER
			ELSE MAIN.INVOICE_NUMBER END AS INVOICE_NO
	 , ITEM.ITEMS_AMT AS INVOICE_AMT
	 , ITEM.ITEMS_AMT + ITEM.ESLITE_DSC_AMT AS SALES_AMT
	 , ITEM.ITEMS_AMT_NOTAX AS SALES_AMT_NOTAX 
	 , ITEM.ITEMS_DISC_AMT AS TOT_DISC_AMT
	 , ITEM.ITEMS_AMT + ITEM.ESLITE_DSC_AMT AS SL_EC_AMT
	 , CARD.CARDCODE AS CARD_ID
	 , CARD.CARDNUMBER AS MEMBER_ID
	 , SUB.SP19_CARD_LEVEL AS MEMBER_EN
	 , SUB.SP19_PROMOTION_CARD_LEVEL AS MEMBER_EN_PARENT
	 , MAIN.SHIPPING_TYPE AS SHIP_TYPE
	 , NULL AS RTN_DATE
	 , NULL AS RTN_BILLING_DATE
	 , ACC.ACCOUNT AS EC_CUST
	 , '' AS SL_KEY_O
	 , 1 AS FLAG
	 , ITEM.ITEMS_TAX_AMT AS TAX
	 , MAIN.TAXID AS BUYER_ID
	 , MAIN.ORDER_NUMBER AS ORDER_NUMBER
	 , CASE SUB.IS_B2B_ORDER WHEN 1 THEN 'Y'
							  WHEN 0 THEN 'N'
							  ELSE '' END AS IS_B2B_ORDER
	 , MAIN.RECIPIENT_STORE_ID
	 , MAIN.RECIPIENT_STORE_NAME
	 , MAIN.RECIPIENT_STORE_TYPE
	 , MAIN.RECIPIENT_COUNTRY
	 , MAIN.RECIPIENT_ZIP
	 , MAIN.RECIPIENT_DISTRICT
	 , MAIN.RECIPIENT_ADDRESS
	 , '' AS RETURN_TYPE
	 , SUB.ETL_DATE 		
	 , NULL AS FOR_GROUP
	 , MAIN.RECIPIENT_CITY
	 , ITEM.ESLITE_DSC_AMT AS DSC_POINT_AMT
	 , MAIN.ORDER_SOURCE 
     , SUB.arrived_at AS ARRIVAL_DATE
     , SUB.finished_at AS PICKUP_DATE
FROM ECS2.SUB_ORDERS SUB

LEFT JOIN ECS2.MAIN_ORDERS MAIN
  ON SUB.MAIN_ORDER_ID = MAIN.ID
  
LEFT JOIN (SELECT MAIN_ORDER_ID , SUB_ORDER_ID , MAX(ETL_DATE) AS ETL_DATE , SUM(ACTUALLY_PRICE) AS ITEMS_AMT, SUM(ISNULL(UNTAXAMOUNT,0)) AS ITEMS_AMT_NOTAX ,
             SUM(DISCOUNT_AMOUNT_PRICE) AS ITEMS_DISC_AMT , SUM(ISNULL(TAX,0)) AS ITEMS_TAX_AMT , SUM(ESLITE_DISCOUNT_AMOUNT_PRICE) AS ESLITE_DSC_AMT
		FROM ECS2.SUB_ORDER_ITEMS
       GROUP BY MAIN_ORDER_ID , SUB_ORDER_ID  ) ITEM
  ON ITEM.SUB_ORDER_ID = SUB.ID
 
OUTER APPLY (SELECT TOP 1 * FROM EC.MEMBER_ESLITECARDINF CARD
			  WHERE MAIN.MEMBER_UUID = CARD.USERID 
			    AND CARD.DELETE_MARK IS NULL
			  ORDER BY LASTMODIFYDATE DESC ) CARD
  
LEFT JOIN EC.ACCOUNT_DATA ACC
  ON MAIN.MEMBER_UUID = ACC.ACCOUNT_SEQNO
  
WHERE SUB.ESP_INVOICE_NUMBER_CREATED_DATE &gt;= concat(cast((YEAR(GETDATE()) - 5 + 0) as varchar),'0101')
  AND ISNULL(SUB.DELETE_MARK,0) &lt;&gt; 1
  AND ( SUB.ETL_DATE &gt;= cast(cast('2025-03-12 10:25:45.658000000' as date) as datetime)
  OR MAIN.ETL_DATE &gt;= cast(cast('2025-03-12 10:25:45.658000000' as date) as datetime)
  OR ITEM.ETL_DATE &gt;= cast(cast('2025-03-12 10:25:45.658000000' as date) as datetime)
  OR CARD.ETL_DATE &gt;= cast(cast('2025-03-12 10:25:45.658000000' as date) as datetime) )
  
union

SELECT CONCAT('N',R_SUB.MIN_ID) AS SL_KEY
     , SUB.ORDER_NUMBER AS TRANS_SEQNO
	 , '-1' AS TRANS_TYPE
	 , R_SUB.CREATED_AT AS TRANS_DATE
	 , R_SUB.ESP_INVOICE_NUMBER_REVOKED_DATE AS INVOICE_DATE
	 , MAIN.CREATED_AT AS ORDER_DATE
	 , MAIN.INVOICE_TYPE AS INV_TYPE
	 , CASE WHEN MAIN.INVOICE_NUMBER = ''
	        THEN SUB.ESP_INVOICE_NUMBER
			ELSE MAIN.INVOICE_NUMBER END AS INVOICE_NO
	 , R_SUB.R_SALES_AMT AS INVOICE_AMT
	 , R_SUB.R_SALES_AMT + R_SUB.ESLITE_POINT AS SALES_AMT
	 , R_SUB.R_NOTAX_AMT AS SALES_AMT_NOTAX 
	 , R_SUB.R_DISC_AMT AS TOT_DISC_AMT
	 , R_SUB.R_SALES_AMT + R_SUB.ESLITE_POINT AS SL_EC_AMT
	 , CARD.CARDCODE AS CARD_ID
	 , CARD.CARDNUMBER AS MEMBER_ID
	 , SUB.SP19_CARD_LEVEL AS MEMBER_EN
	 , SUB.SP19_PROMOTION_CARD_LEVEL AS MEMBER_EN_PARENT
	 , '' AS SHIP_TYPE
	 , R_SUB.CREATED_AT AS RTN_DATE
	 , SUB.ESP_INVOICE_NUMBER_CREATED_DATE AS RTN_BILLING_DATE
	 , ACC.ACCOUNT AS EC_CUST
	 , CONCAT('P',R_SUB.SUB_ORDER_ID) AS SL_KEY_O
	 , '-1' AS FLAG
	 , R_SUB.R_TAX_AMT AS TAX
	 , MAIN.TAXID AS BUYER_ID
	 , MAIN.ORDER_NUMBER AS ORDER_NUMBER
	 , CASE SUB.IS_B2B_ORDER WHEN 1 THEN 'Y'
							  WHEN 0 THEN 'N'
							  ELSE '' END AS IS_B2B_ORDER
	 , '' AS RECIPIENT_STORE_ID
	 , '' AS RECIPIENT_STORE_NAME
	 , '' AS RECIPIENT_STORE_TYPE
	 , '' AS RECIPIENT_COUNTRY
	 , '' AS RECIPIENT_ZIP
	 , '' AS RECIPIENT_DISTRICT
	 , '' AS RECIPIENT_ADDRESS
	 , R_SUB.RETURN_TYPE AS RETURN_TYPE
	 , R_SUB.ETL_DATE
	 , R_SUB.FOR_GROUP
	 , '' AS RECIPIENT_CITY
	 , R_SUB.ESLITE_POINT AS DSC_POINT_AMT
	 , MAIN.ORDER_SOURCE 
     , NULL AS ARRIVAL_DATE
     , NULL AS PICKUP_DATE
FROM (SELECT R_SUB.MAIN_ORDER_ID , R_SUB.SUB_ORDER_ID , CONVERT(VARCHAR(10) , R_SUB.CREATED_AT ,111) AS FOR_GROUP , MAX(R_SUB.CREATED_AT) AS CREATED_AT, R_SUB.RETURN_TYPE , MAX(R_SUB.ETL_DATE) AS ETL_DATE , MIN(R_SUB.ID) AS MIN_ID , SUM(R_SUB.PRICE) AS R_SALES_AMT , SUM(ISNULL(R_SUB.TAX,0)) AS R_TAX_AMT ,
             SUM(ISNULL(R_SUB.UNTAXAMOUNT,0)) AS R_NOTAX_AMT , SUM(ISNULL(SUB.DISCOUNT_AMOUNT_PRICE,0)) AS R_DISC_AMT , MIN(ESP_INVOICE_NUMBER_REVOKED_DATE) AS ESP_INVOICE_NUMBER_REVOKED_DATE , SUM(ESLITE_POINT) AS ESLITE_POINT
			 
		FROM ECS2.RETURN_SUB_ORDER_ITEMS R_SUB
		
		LEFT JOIN ECS2.SUB_ORDER_ITEMS SUB
		  ON R_SUB.MAIN_ORDER_ID = SUB.MAIN_ORDER_ID
		 AND R_SUB.SUB_ORDER_ID = SUB.SUB_ORDER_ID
		 AND R_SUB.SUB_ORDER_ITEM_ID = SUB.ID
		 
	   GROUP BY R_SUB.MAIN_ORDER_ID , R_SUB.SUB_ORDER_ID , CONVERT(VARCHAR(10) , R_SUB.CREATED_AT ,111) , R_SUB.RETURN_TYPE ) R_SUB
	   
JOIN ECS2.SUB_ORDERS SUB
  ON R_SUB.MAIN_ORDER_ID = SUB.MAIN_ORDER_ID
 AND R_SUB.SUB_ORDER_ID = SUB.ID
 
JOIN ECS2.MAIN_ORDERS MAIN
  ON R_SUB.MAIN_ORDER_ID = MAIN.ID
  
OUTER APPLY (SELECT TOP 1 * FROM EC.MEMBER_ESLITECARDINF CARD
			  WHERE MAIN.MEMBER_UUID = CARD.USERID 
			    AND CARD.DELETE_MARK IS NULL
			  ORDER BY LASTMODIFYDATE DESC ) CARD
  
LEFT JOIN EC.ACCOUNT_DATA ACC
  ON MAIN.MEMBER_UUID = ACC.ACCOUNT_SEQNO
  
WHERE R_SUB.ETL_DATE &gt;= cast(cast('2025-03-12 10:25:45.658000000' as date) as datetime)
