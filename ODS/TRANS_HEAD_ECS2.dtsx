                <property
                  dataType="System.String"
                  description="要執行的 SQL 命令。"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
	 -- ** 交易基礎資訊PK+序(EC沒有店日機)
	   SL_KEY
	 , tmp.OrderId as TRANS_SEQNO
	 -- ** 交易金額 &amp; 相關欄位
	 , TRANS_TYPE		AS FLAG              -- 正/負向交易
	 , TRANS_TYPE                            -- 交易別
	 , SAP_BILLING_DATE                      -- ETL轉換TRANS_DATE
     , orders.OrderDate AS ORDER_DATE        -- 下單日期      
	 , GUI_NO           AS INVOICE_NO        -- 發票號碼          
	 , INVOICE_TOTAL3   AS INVOICE_AMT       -- 發票金額
	 , INVOICE_TOTAL3   AS SALES_AMT        -- 銷售總額(含稅)
	 , isnull(INVOICE_TOTAL3,0)-isnull(VAT_AMOUNT,0) AS SALES_AMT_NOTAX -- 交易金額(未稅)
	 , VAT_AMOUNT       AS TAX               -- 稅額
	 , INVOICE_TOTAL3   AS SL_EC_AMT         -- EC交易金額
	 , CASE WHEN PAYMENT_TYPE = 'Z14' 
		    THEN orders.TotalDiscount 
			ELSE 0 END AS DSC_POINT_AMT      -- 點數折抵金額
	 , orders.CouponAmount as DSC_COUPON_AMT -- 贈券折抵金額
	 , orders.ShipType as SHIP_TYPE          -- 取貨方式
	 -- ** 退貨資訊
	 , CASE WHEN TRANS_TYPE = -1 
	        THEN IMPORT_TIME 
			ELSE null END AS RTN_DATE         -- 退貨日期
	 , SAP_BILLING_DATE_O as RTN_BILLING_DATE -- 歸結日期
	 , SL_KEY_O                               -- 原始交易主鍵
	 -- ** 會員/過路客
	 , ES_CARD_NO as CARD_ID
	 , ES_MEMBER_NO as MEMBER_ID	 
	 , acc.Account as EC_CUST
	 , ES_CARD_Level1 AS MEMBER_EN
	 , ES_CARD_Level2 AS MEMBER_EN_PARENT
from
(
select concat(suborders.SUB_ORDER_NUMBER,suborders.INVOICE_NUMBER) as SL_KEY
     , suborders.OrderId
	 , 1 as TRANS_TYPE
	 , SAP_BILLING_DATE
	 , GUI_NO
	 , INVOICE_TOTAL3
	 , VAT_AMOUNT
	 , PAYMENT_TYPE
	 , ES_CARD_NO
	 , ES_MEMBER_NO
	 , IMPORT_TIME
	 , null as SAP_BILLING_DATE_O
	 , null AS SL_KEY_O
	 , ES_CARD_Level1
     , ES_CARD_Level2
from EC.ES_SAP_SUBORDERS suborders
where suborders.SAP_BILLING_DATE &gt;= concat(cast((YEAR(GETDATE()) - 5 + ?) as varchar),'0101')
union
select concat(suborders.SUB_ORDER_NUMBER,suborders.INVOICE_NUMBER) as SL_KEY
     , suborders.OrderId
	 , -1 as TRANS_TYPE
	 , suborders.SAP_BILLING_DATE
	 , suborders.GUI_NO
	 , INVOICE_TOTAL3
	 , VAT_AMOUNT
	 , PAYMENT_TYPE
	 , main.ES_CARD_NO
	 , main.ES_MEMBER_NO
	 , IMPORT_TIME
	 , main.SAP_BILLING_DATE as SAP_BILLING_DATE_O
	 , concat(main.SUB_ORDER_NUMBER,main.INVOICE_NUMBER) AS SL_KEY_O
	 , main.ES_CARD_Level1
     , main.ES_CARD_Level2
from EC.ES_SAP_SUBORDERS_CANCEL suborders
left join (select SUB_ORDER_NUMBER,GUI_NO,SAP_BILLING_DATE,INVOICE_NUMBER,ES_CARD_Level1,ES_CARD_Level2,ES_CARD_NO,ES_MEMBER_NO from EC.ES_SAP_SUBORDERS) main 
  on suborders.SUB_ORDER_NUMBER = main.SUB_ORDER_NUMBER
 and suborders.GUI_NO = main.GUI_NO
where suborders.SAP_BILLING_DATE &gt;= concat(cast((YEAR(GETDATE()) - 5 + ?) as varchar),'0101')
) tmp 
left join EC.O_Orders orders on tmp.OrderId = orders.OrderID
left join EC.Account_Data acc on orders.UserID = acc.Account_SeqNo</property>
