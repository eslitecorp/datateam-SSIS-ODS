        #endregion


        /// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {
            String sLogDate = Dts.Variables["$Package::sLogDate"].Value.ToString();
            String eLogDate = Dts.Variables["$Package::eLogDate"].Value.ToString();

            bool isMail = false;
            string pattern = @"[0-9]{4}-[0-9]{2}-[0-9]{2}";
            StringBuilder mailMsg = new StringBuilder();

            // null or empty
            if (String.IsNullOrEmpty(sLogDate) || String.IsNullOrEmpty(eLogDate))
            {
                isMail = true;
                if (String.IsNullOrEmpty(sLogDate))
                {
                    mailMsg.Append("指定區間參數 sLogDate 不得為空值\r\n");
                }
                if (String.IsNullOrEmpty(eLogDate))
                {
                    mailMsg.Append("指定區間參數 eLogDate 不得為空值\r\n");
                }
            }
            else if (!Regex.IsMatch(sLogDate, pattern) || !Regex.IsMatch(eLogDate, pattern))
            {
                isMail = true;
                // pattern
                if (!Regex.IsMatch(sLogDate, pattern))
                {
                    mailMsg.Append("指定區間參數 sLogDate 不符合格式(例：2020-01-01)\r\n");
                }
                if (!Regex.IsMatch(eLogDate, pattern))
                {
                    mailMsg.Append("指定區間參數 eLogDate 不符合格式(例：2020-01-01)\r\n");
                }
            }
            // sLogDate <= eLogDate
            else if (DateTime.Parse(sLogDate) > DateTime.Parse(eLogDate))
            {
                isMail = true;
                mailMsg.Append("指定區間參數 sLogDate的日期，不得大於 eLogDate的日期 \r\n");
            }

            if (isMail)
                Dts.Variables["User::mailSubject"].Value = "「ODS」數位軌跡-會員登入歷程 指定區間參數設定錯誤";

            Dts.Variables["User::mailMsg"].Value = mailMsg.ToString();

            Dts.TaskResult = (int)ScriptResults.Success;
        }

        #region ScriptResults declaration
