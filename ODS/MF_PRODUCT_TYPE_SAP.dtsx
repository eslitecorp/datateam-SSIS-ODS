                <property
                  dataType="System.String"
                  description="指定包含用於開啟資料列集之資料庫物件名稱的變數。"
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="要執行的 SQL 命令。"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT D_ProductType_MC3.MC3 AS MC3_CB
            , D_ProductType_MC3.Descripton1 AS MC3_Name
	, D_ProductType_MID.MC6, D_ProductType_MID.MC6_Name	
	, D_ProductType_SMALL.MATKL, D_ProductType_SMALL.WGBEZ
	, D_ProductType_MC4.MC4, D_ProductType_MC4.MC4_Name
	, SP44.ZHOBBY AS INT_ID
	, SP44.ZHOBBY_NAME AS INT_NAME
	, SP44.ZMKT_MAJOR AS CB_MCH4
	, SP44.ZMKT_MAJ_NAME AS CB_MCH4_NAME
	, SP44.ZMKT_MEDIUM AS CB_MCH5
	, SP44.ZMKT_MED_NAME AS CB_MCH5_NAME
	, 'CB' as SRC_SYS
FROM
(
	SELECT SUBSTRING(MATKL,1,3) AS MC3, SUBSTRING(MATKL,1,4) AS MC4, SUBSTRING(MATKL,1,6) AS MC6, MATKL, WGBEZ
	FROM SAP_EADB.SP06
) D_ProductType_SMALL 
LEFT JOIN (
	SELECT MC3, Descripton1
	FROM SAP_EADB.SPM08
	GROUP BY MC3, Descripton1
) D_ProductType_MC3 ON D_ProductType_MC3.MC3 = D_ProductType_SMALL.MC3	-- 小類 tmp table
LEFT JOIN (
	SELECT MC3, TRIM(MC3) + TRIM(MC4) AS MC6, TRIM(Descripton1) + TRIM(Descripton2) AS MC6_Name, substring(TRIM(MC3) + TRIM(MC4), 1, 4) MC4
	FROM SAP_EADB.SPM08
	WHERE LEN(MC4) = 3
) D_ProductType_MID ON D_ProductType_SMALL.MC3 = D_ProductType_MID.MC3	and D_ProductType_SMALL.MC6 = D_ProductType_MID.MC6 -- 中類 tmp table
LEFT JOIN (
	SELECT MC3, TRIM(MC3) + TRIM(MC4) AS MC4, TRIM(Descripton1) + TRIM(Descripton2) AS MC4_Name
	FROM SAP_EADB.SPM08
	WHERE LEN(MC4) = 1
) D_ProductType_MC4 ON D_ProductType_SMALL.MC3 = D_ProductType_MC4.MC3 and D_ProductType_SMALL.MC4 = D_ProductType_MC4.MC4-- MC4 tmp table
LEFT JOIN SAP_EADB.SP44 ON D_ProductType_SMALL.MATKL = SP44.MATKL</property>
