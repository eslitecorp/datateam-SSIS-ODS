                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select offer.OFFER_ID --Offer ID
     , offer.MEMBER_ID --會員編號
  , null as MEMBER_EN --會員身分卡別
  , MIN_CLICK_DATE --點擊日期
