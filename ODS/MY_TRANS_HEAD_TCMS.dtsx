                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
	-- ** 交易基礎資訊PK+店日機序
      SL_KEY				    -- PK
	, UPPER(S_NO) AS WERKS				-- 門市
	, SL_DATE AS BUSINESSDATE   -- 營業日期
	, ID_NO AS POSNO			-- 機號
	, SL_NO AS TRANS_SEQNO		-- 序號
	-- ** 交易金額 &amp; 相關欄位
	, 1 AS FLAG					-- 正/負向交易
	, SL_INKIND AS TRANS_TYPE	-- 交易別
	, SL_DATE					-- ETL組合TRANS_DATE
	, SL_TIME					-- ETL組合TRANS_DATE	
	, SL_INVNO AS INVOICE_NO	-- 發票號碼
	, SL_INVAMT AS INVOICE_AMT	-- 發票金額
	, SL_TOTAL AS SALES_AMT	    -- 銷售總額(含稅)
	, SL_AMT AS SALES_AMT_NOTAX -- 交易金額(未稅)
	, SL_TODSC AS TOT_DISC_AMT	-- 總額讓額
	, DSC_AMT AS DSC_POINT_AMT	-- 點數折抵金額
	, DSC_AMT AS DSC_COUPON_AMT -- 贈券折抵金額
	-- ** 地點
	, SH_ID AS LIFNR_COUNTER
	-- ** 會員/過路客
	, CC_NO AS CARD_ID
	, C_NO AS MEMBER_ID
	, CRD_NO AS MEMBER_EN
	, CASE WHEN UPPER(S_NO) = 'N001' THEN UPPER(S_NO) ELSE 'LMS_MRK' END AS SRC_SYS
FROM LMS.CUSL_H
where (UPPER(S_NO) = 'N001' or ID_NO = '998' ) and convert(varchar, cast(SL_DATE as date), 112) &gt;= concat(cast((YEAR(GETDATE()) - 5 + ?) as varchar),'0101')</property>
