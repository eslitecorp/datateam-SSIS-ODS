	   , DETAIL.PRICEFREEDISCAMT	AS PRICE_FREE_DISC_AMT
	   , DETAIL.TOTDISCAMT			AS TOT_DISC_AMT
	   , DETAIL.SALESAMT			AS SALES_AMT
	   
	   , 'TPOS'						AS SRC_SYS
