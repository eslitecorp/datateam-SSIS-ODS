	   , DETAIL.MEALNAME			AS MEAL_NAME
	   , DETAIL.MSMORDERFLAG		AS MSM_FLAG
	   , DETAIL.MSMORDERTYPE		AS MSM_TYPE
	   , DETAIL.QUANTITY			AS QUANTITY
