              <properties>
                <property
                  dataType="System.Int32"
                  description="命令逾時之前的秒數。值為 0 表示無限逾時。"
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="指定用於開啟資料列集之資料庫物件的名稱。"
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="指定包含用於開啟資料列集之資料庫物件名稱的變數。"
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="要執行的 SQL 命令。"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
	 -- ** 交易基礎資訊PK+店日機序
	   concat(head.StoreID,replace(head.BusinessDate,'/',''),head.PosNo,head.TradeSeqNo) AS SL_KEY -- PK
     , head.StoreID       AS WERKS        -- 門市
	 , head.BusinessDate  AS BUSINESSDATE -- 營業日期
	 , head.PosNo         AS POSNO        -- 機號
	 , head.TradeSeqNo    AS TRANS_SEQNO  -- 序號
	 , detail.ItemSeqNo   AS ITEM_SEQ     -- 明細流水號
	 -- ** 交易金額、數量 &amp; 相關欄位
	 , case when head.TranArea in ('102','103','110') 
			THEN -1 ELSE 1 END AS FLAG                -- 正/負向交易
	 , head.TradeTime AS TRANS_DATE                         -- 交易時間
 , detail.Quantity         as QUANTITY            -- 銷售數量		 
	 , detail.UnitPrice        AS UNIT_PRICE          -- 單品原價
	 , detail.SalePrice        AS SALE_PRICE          -- 單品售價
	 , detail.UseActDiscAmt    AS USE_ACT_DISC_AMT    -- 單品折讓額
	 , detail.PriceFreeDiscAmt AS PRICE_FREE_DISC_AMT -- 單品折價金額
	 , detail.TotDiscAmt       as TOT_DISC_AMT        -- 折讓額
	 , detail.SalesAmt         as SALES_AMT			  -- 含稅銷售金額
	 , detail.TaxAmt           as TAX                 -- 稅額
	 -- ** 商品資訊 &amp; 活動代碼欄位
	 , detail.ItemCode       as MATNR         -- 商品料號
	 , head.TenantNo         AS LIFNR_COUNTER -- 專櫃廠商代碼
	 , detail.TenantItemCat3 AS COUITEMCAT    -- 專櫃商品屬性
	 , CASE WHEN isnull(CatID3,'') &lt;&gt; '' THEN CatID3
			WHEN isnull(FSCatID,'') &lt;&gt; '' THEN FSCatID
			ELSE NULL END	 AS MATKL         -- 小類ID
	 , detail.ItemBarCode    AS ITEM_BARCODE  -- 商品條碼(ISBN/菜單編號)
	 , detail.ItemName       AS PROD_LNAME    -- 產品長名
	 , detail.MealName       AS MEAL_NAME     -- 菜單名稱
	 , detail.MsmOrderFlag   AS MSM_FLAG      -- 是否為MSM客訂單商品
	 , detail.MsmOrderFlag   AS MSM_TYPE      -- 客訂類別
from POSNEC.PosTradeM head
join POSNEC.PosTradeD detail
  ON head.StoreID = detail.StoreID
 and head.BusinessDate = detail.BusinessDate
 and head.PosNo = detail.PosNo
 and head.TradeSeqNo = detail.TradeSeqNo
where head.TranArea not in ('200','201')</property>
