	   , HEAD.BUSINESSDATE  		AS BUSINESSDATE 
	   , HEAD.POSNO         		AS POSNO        
	   , HEAD.TRADESEQNO    		AS TRANS_SEQNO  
	   , DETAIL.ITEMSEQNO   		AS ITEM_SEQ_NO
	   , RIGHT('0000000' + DETAIL.ITEMCODE , 18) AS MATNR
	   , HEAD.TENANTNO 				AS LIFNR_COUNTER
	   , DETAIL.TENANTITEMCAT3 		AS COUITEMCAT
       , CASE WHEN CatID3 &lt;&gt; ''  THEN CatID3
              WHEN FSCatID &lt;&gt; '' THEN FSCatID
              ELSE NULL END         AS MATKL
