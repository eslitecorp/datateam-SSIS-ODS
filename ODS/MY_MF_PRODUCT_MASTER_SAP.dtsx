&#x9;  ,SP09.NAME1 AS LIFNR_NAME&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZVERS2) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZVERS2,1,CHARINDEX(':',SP01.ZZVERS2)-1)&#xA;&#x9;&#x9;ELSE SP01.ZZVERS2 END) AS ZZVERS2&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZVERS2) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZVERS2,CHARINDEX(':',SP01.ZZVERS2)+1,LEN(SP01.ZZVERS2)-CHARINDEX(':',SP01.ZZVERS2))&#xA;&#x9;&#x9;ELSE SP01.ZZVERS2 END) AS ZZVERS2_NAME&#xA;      ,SP01.ERSDA &#xA;      ,SP01.ZZFPDAT&#xA;&#x9;  ,CASE WHEN SUBSTRING(SP01.EKGRP,1,1) IN ('A','B','D','F') THEN substring(SP01.EKGRP,1,1)  ELSE null END AS EKGRP2&#xA;&#x9;  ,CASE WHEN SUBSTRING(SP01.EKGRP,1,1) IN ('A','B','D','F') THEN '書' ELSE '非書' END AS EKGRP2_NAME&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZRAT) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZRAT,1,CHARINDEX(':',SP01.ZZRAT)-1)&#xA;&#x9;&#x9;ELSE SP01.ZZRAT END) AS ZZRAT&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZRAT) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZRAT,CHARINDEX(':',SP01.ZZRAT)+1,LEN(SP01.ZZRAT)-CHARINDEX(':',SP01.ZZRAT))&#xA;&#x9;&#x9;ELSE SP01.ZZRAT END) AS ZZRAT_NAME    &#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZLANG) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZLANG,1,CHARINDEX(':',SP01.ZZLANG)-1)&#xA;&#x9;&#x9;ELSE SP01.ZZLANG END) AS ZZLANG&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZLANG) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZLANG,CHARINDEX(':',SP01.ZZLANG)+1,LEN(SP01.ZZLANG)-CHARINDEX(':',SP01.ZZLANG))&#xA;&#x9;&#x9;ELSE SP01.ZZLANG END) AS ZZLANG_NAME&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.LFRHY) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.LFRHY,1,CHARINDEX(':',SP01.LFRHY)-1)&#xA;&#x9;&#x9;ELSE SP01.LFRHY END) AS LFRHY&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.LFRHY) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.LFRHY,CHARINDEX(':',SP01.LFRHY)+1,LEN(SP01.LFRHY)-CHARINDEX(':',SP01.LFRHY))&#xA;&#x9;&#x9;ELSE SP01.LFRHY END) AS LFRHY_NAME&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZPR) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZPR,1,CHARINDEX(':',SP01.ZZPR)-1)&#xA;&#x9;&#x9;ELSE SP01.ZZPR END) AS ZZPR&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZPR) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZPR,CHARINDEX(':',SP01.ZZPR)+1,LEN(SP01.ZZPR)-CHARINDEX(':',SP01.ZZPR))&#xA;&#x9;&#x9;ELSE SP01.ZZPR END) AS ZZPR_NAME&#xA;      ,SP01.NETPR &#xA;      ,CAST(SP01.MEINS AS VARCHAR) AS MEINS&#xA;      ,SP01.MINBM &#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZIMCHK) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZIMCHK,1,CHARINDEX(':',SP01.ZZIMCHK)-1)&#xA;&#x9;&#x9;ELSE SP01.ZZIMCHK END) AS ZZIMCHK&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZIMCHK) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZIMCHK,CHARINDEX(':',SP01.ZZIMCHK)+1,LEN(SP01.ZZIMCHK)-CHARINDEX(':',SP01.ZZIMCHK))&#xA;&#x9;&#x9;ELSE SP01.ZZIMCHK END) AS ZZIMCHK_NAME&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZCTYP) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZCTYP,1,CHARINDEX(':',SP01.ZZCTYP)-1)&#xA;&#x9;&#x9;ELSE SP01.ZZCTYP END) AS ZZCTYP&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.ZZCTYP) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.ZZCTYP,CHARINDEX(':',SP01.ZZCTYP)+1,LEN(SP01.ZZCTYP)-CHARINDEX(':',SP01.ZZCTYP))&#xA;&#x9;&#x9;ELSE SP01.ZZCTYP END) AS ZZCTYP_NAME&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.VTEXT) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.VTEXT,1,CHARINDEX(':',SP01.VTEXT)-1)&#xA;&#x9;&#x9;ELSE SP01.VTEXT END) AS VTEXT&#xA;&#x9;,(CASE WHEN CHARINDEX(':',SP01.VTEXT) &gt; 0&#xA;&#x9;&#x9;THEN SUBSTRING(SP01.VTEXT,CHARINDEX(':',SP01.VTEXT)+1,LEN(SP01.VTEXT)-CHARINDEX(':',SP01.VTEXT))&#xA;&#x9;&#x9;ELSE SP01.VTEXT END) AS VTEXT_NAME&#xA;      , SP01.ZZTHNUM &#xA;      , SP01.IDNLF &#xA;&#x9;  , SP24_PB.KBETR as KBETR_PB&#xA;&#x9;  , SP24_RA.KBETR as KBETR_RA&#xA;&#x9;  , SP24_ZCOV.KBETR as KBETR_ZCOV&#xA;&#x9;  , SP26.KBETR as KBETR_26&#x9;&#x9;&#xA;&#x9;  , SP01.ZZKEY AS KEYWORD&#xA;&#x9;  ,CASE '&quot; + @[$Package::ekorg] + &quot;'&#xA;&#x9;&#x9;WHEN 'EMDB' THEN 'CB'&#xA;&#x9;&#x9;WHEN 'EMRD' THEN 'SD'&#xA;&#x9;&#x9;ELSE null END as SRC_SYS&#xA;&#xA;&#xA;&#xA;, SP02.EAN11 AS EAN&#xA;FROM [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP01] SP01&#xA;&#xA;LEFT JOIN [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP09] SP09 ON SP01.LIFNR = SP09.LIFNR&#xA;&#xA;LEFT JOIN [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP09] SP09_K ON SP01.KOLIF = SP09_K.LIFNR&#xA;&#xA;LEFT JOIN (SELECT *&#xA;&#x9;&#x9;FROM [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP24] SP24&#xA;&#x9;&#x9;WHERE KSCHL = 'PB00' AND DATAB = (&#xA;&#x9;&#x9;select MAX(DATAB)&#xA;&#x9;&#x9;from [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP24] SP24_MD&#xA;&#x9;&#x9;WHERE SP24.MATNR = SP24_MD.MATNR AND SP24.WERKS = SP24_MD.WERKS AND SP24.KSCHL = SP24_MD.KSCHL AND SP24.LIFNR = SP24_MD.LIFNR)) SP24_PB ON SP01.MATNR = SP24_PB.MATNR AND SP01.LIFNR = SP24_PB.LIFNR&#xA;LEFT JOIN (SELECT *&#xA;&#x9;&#x9;FROM [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP24] SP24&#xA;&#x9;&#x9;WHERE KSCHL = 'RA00' AND DATAB = (&#xA;&#x9;&#x9;select MAX(DATAB)&#xA;&#x9;&#x9;from [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP24] SP24_MD&#xA;&#x9;&#x9;WHERE SP24.MATNR = SP24_MD.MATNR AND SP24.WERKS = SP24_MD.WERKS AND SP24.KSCHL = SP24_MD.KSCHL AND SP24.LIFNR = SP24_MD.LIFNR)) SP24_RA ON SP01.MATNR = SP24_RA.MATNR AND SP01.LIFNR = SP24_RA.LIFNR&#xA;LEFT JOIN (SELECT *&#xA;&#x9;&#x9;FROM [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP24] SP24&#xA;&#x9;&#x9;WHERE KSCHL = 'ZCOV' AND DATAB = (&#xA;&#x9;&#x9;select MAX(DATAB)&#xA;&#x9;&#x9;from [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP24] SP24_MD&#xA;&#x9;&#x9;WHERE SP24.MATNR = SP24_MD.MATNR AND SP24.WERKS = SP24_MD.WERKS AND SP24.KSCHL = SP24_MD.KSCHL AND SP24.LIFNR = SP24_MD.LIFNR)) SP24_ZCOV ON SP01.MATNR = SP24_ZCOV.MATNR AND SP01.LIFNR = SP24_ZCOV.LIFNR&#xA;&#xA;LEFT JOIN (SELECT *&#xA;&#x9;&#x9;&#x9;FROM [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP26] SP26&#xA;&#x9;&#x9;&#x9;WHERE DATAB = (&#xA;&#x9;&#x9;&#x9;select MAX(DATAB)&#xA;&#x9;&#x9;&#x9;from [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP26] SP26_MD&#xA;&#x9;&#x9;&#x9;WHERE SP26.MATNR = SP26_MD.MATNR AND SP26.WERKS = SP26_MD.WERKS)) SP26 ON SP01.MATNR = SP26.MATNR AND SP01.WERKS = SP26.WERKS&#xA;&#xA;&#xA;LEFT JOIN ( SELECT SP01.WERKS, SP01.MATNR, SP31.EKSGB, SP31.EKSTX&#xA;&#x9;&#x9;  FROM [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP01]&#xA;&#x9;&#x9;&#x9;  LEFT JOIN [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP29] ON SUBSTRING(SP01.EKGRP,1,3) = SP29.EKGRP  and SP29.EKORG = '&quot;+ @[$Package::ekorg] +&quot;'&#xA;&#x9;&#x9;&#x9;  LEFT JOIN [StagingDB_MY].[SAP_&quot;+ @[$Package::ekorg] +&quot;].[SP31] ON SP29.EKSGB = SP31.EKSGB&#xA;&#x9; ) SP31 ON SP01.WERKS = SP31.WERKS AND SP01.MATNR = SP31.MATNR&#xA;OUTER APPLY(select top 1 EAN11&#xA;&#x9; , CASE EANTP WHEN 'HE' THEN 1&#xA;&#x9;              WHEN 'ZI' THEN 2&#xA;&#x9;&#x9;&#x9;&#x9;  WHEN 'Z4' THEN 3&#xA;&#x9;&#x9;&#x9;&#x9;  WHEN 'Z5' THEN 4&#xA;&#x9;&#x9;&#x9;&#x9;  WHEN 'Z6' THEN 5&#xA;&#x9;&#x9;&#x9;&#x9;  WHEN 'Z7' THEN 6&#xA;&#x9;&#x9;&#x9;&#x9;  WHEN 'Z8' THEN 7&#xA;&#x9;&#x9;&#x9;&#x9;  ELSE 99 END AS TY_ORDER&#xA;from STAGINGDB_MY.SAP_&quot;+ @[$Package::ekorg] +&quot;.SP02 &#xA;where ISNULL(EAN11,'') &lt;&gt; '' and MATNR = SP01.MATNR&#xA;order by TY_ORDER, ETL_DATE desc) SP02&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="stageSql">
      <DTS:VariableValue
        DTS:DataType="8">SELECT 'EMDB' AS PROCURE_ID
	  
              ,SP01.WERKS	-- 門市
      
              ,SP01.MATNR 
      
              ,SP01.MAKTX as PROD_SNAME
     
              ,SP01.ZZCT as PROD_LNAME
      
              ,SP01.ZZSUBT as PROD_BRAND 
	  
              ,SP01.ZZSUBT as PROD_BRAND_SUB 
	  
              ,SP01.MATKL 
	  
              ,CASE WHEN ISNULL(SP01.MATKL,'') &lt;&gt; '' THEN SUBSTRING(SP01.MATKL,1,3) ELSE '' END AS MC3

			  
              ,CASE WHEN ISNULL(SP01.MATKL,'') &lt;&gt; '' THEN SUBSTRING(SP01.MATKL,1,4) ELSE '' END AS MC4

			  
              ,CASE WHEN ISNULL(SP01.MATKL,'') &lt;&gt; '' THEN SUBSTRING(SP01.MATKL,1,6) ELSE '' END AS MC6
              ,SP31.EKSGB
	  
              ,SP31.EKSTX
      
              ,SP01.MTART 
	
              ,(CASE WHEN CHARINDEX(':',SP01.EKGRP) &gt; 0
		THEN SUBSTRING(SP01.EKGRP,1,CHARINDEX(':',SP01.EKGRP)-1)
		ELSE SP01.EKGRP END) AS EKGRP
	
              ,(CASE WHEN CHARINDEX(':',SP01.EKGRP) &gt; 0
		THEN SUBSTRING(SP01.EKGRP,CHARINDEX(':',SP01.EKGRP)+1,LEN(SP01.EKGRP)-CHARINDEX(':',SP01.EKGRP))
		ELSE SP01.EKGRP END) AS EKGRP_NAME
	
              ,(CASE WHEN CHARINDEX(':',SP01.BWSCL) &gt; 0
		THEN SUBSTRING(SP01.BWSCL,1,CHARINDEX(':',SP01.BWSCL)-1)
		ELSE SP01.BWSCL END) AS BWSCL

              ,(CASE WHEN CHARINDEX(':',SP01.BWSCL) &gt; 0
		THEN SUBSTRING(SP01.BWSCL,CHARINDEX(':',SP01.BWSCL)+1,LEN(SP01.BWSCL)-CHARINDEX(':',SP01.BWSCL))
		ELSE SP01.BWSCL END) AS BWSCL_NAME
	
              ,(CASE WHEN CHARINDEX(':',SP01.ZZVPURO) &gt; 0
		THEN SUBSTRING(SP01.ZZVPURO,1,CHARINDEX(':',SP01.ZZVPURO)-1)
		ELSE SP01.ZZVPURO END) AS ZZVPURO
	
              ,(CASE WHEN CHARINDEX(':',SP01.ZZVPURO) &gt; 0
		THEN SUBSTRING(SP01.ZZVPURO,CHARINDEX(':',SP01.ZZVPURO)+1,LEN(SP01.ZZVPURO)-CHARINDEX(':',SP01.ZZVPURO))
		ELSE SP01.ZZVPURO END) AS ZZVPURO_NAME
	
              ,(CASE WHEN CHARINDEX(':',SP01.ZZCONSI) &gt; 0
