	,(CASE WHEN CHARINDEX(':',SP01.ZZTHEC) &gt; 0
		THEN SUBSTRING(SP01.ZZTHEC,CHARINDEX(':',SP01.ZZTHEC)+1,LEN(SP01.ZZTHEC)-CHARINDEX(':',SP01.ZZTHEC))
		ELSE SP01.ZZTHEC END) AS ZZTHEC_NAME
	  , ZZPDATE
      ,KOLIF 
	  , SP09_K.NAME1 as KOLIF_NAME
	  , CASE WHEN substring(SP01.EKGRP,1,1) IN ('A', 'B', 'D', 'E', 'F') THEN substring(SP01.EKGRP,1,1) 
