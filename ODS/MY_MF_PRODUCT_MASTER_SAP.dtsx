		THEN SUBSTRING(SP01.ZZINTYP,CHARINDEX(':',SP01.ZZINTYP)+1,LEN(SP01.ZZINTYP)-CHARINDEX(':',SP01.ZZINTYP))
		ELSE SP01.ZZINTYP END) AS ZZINTYP_NAME
      ,ZZAUTH 
      ,ZZTHEM 
      ,SP01.LIFNR 
	  ,SP09.NAME1 AS LIFNR_NAME
	,(CASE WHEN CHARINDEX(':',SP01.ZZVERS2) &gt; 0
		THEN SUBSTRING(SP01.ZZVERS2,1,CHARINDEX(':',SP01.ZZVERS2)-1)
		ELSE SP01.ZZVERS2 END) AS ZZVERS2
	,(CASE WHEN CHARINDEX(':',SP01.ZZVERS2) &gt; 0
		THEN SUBSTRING(SP01.ZZVERS2,CHARINDEX(':',SP01.ZZVERS2)+1,LEN(SP01.ZZVERS2)-CHARINDEX(':',SP01.ZZVERS2))
		ELSE SP01.ZZVERS2 END) AS ZZVERS2_NAME
      ,ERSDA 
      ,ZZFPDAT
	  ,CASE WHEN SUBSTRING(SP01.EKGRP,1,1) IN ('A','B','D','F') THEN substring(SP01.EKGRP,1,1)  ELSE null END AS EKGRP2
	  ,CASE WHEN SUBSTRING(SP01.EKGRP,1,1) IN ('A','B','D','F') THEN '書' ELSE '非書' END AS EKGRP2_NAME
	,(CASE WHEN CHARINDEX(':',SP01.ZZRAT) &gt; 0
		THEN SUBSTRING(SP01.ZZRAT,1,CHARINDEX(':',SP01.ZZRAT)-1)
