              <properties>
                <property
                  dataType="System.Int32"
                  description="命令逾時之前的秒數。值為 0 表示無限逾時。"
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="指定用於開啟資料列集之資料庫物件的名稱。"
                  name="OpenRowset">[SMS].[SMS]</property>
                <property
                  dataType="System.String"
                  description="指定包含用於開啟資料列集之資料庫物件名稱的變數。"
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="要執行的 SQL 命令。"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select -- ** 交易基礎資訊PK+店日機序
	   concat(head.StoreID,replace(head.BusinessDate,'/',''),head.PosNo,head.TradeSeqNo) AS SL_KEY -- PK
     , head.StoreID         AS WERKS          -- 門市
	 , head.BusinessDate    AS BUSINESSDATE   -- 營業日期
	 , head.PosNo           AS POSNO          -- 機號
	 , head.TradeSeqNo      AS TRANS_SEQNO    -- 序號
	 , item.ItemSeqNo       AS ITEM_SEQ_NO    -- 明細流水號
	 , case when head.TranArea in ('102','103','110') 
			THEN -1 ELSE 1 END AS FLAG        -- 正/負向交易
	 , head.TradeTime AS TRANS_DATE                         -- 交易時間
	 , discount.ProID		AS Pro_ID         -- 檔期編號
	 , ActivityNo			AS Promotion_ID   -- 販促活動代碼
	 , discount.DiscountAmt	AS DISC_AMT       -- 活動折扣金額
	 , discount.SalesAmt	AS DISC_SALES_AMT -- 活動成立金額
	 , head.MemberPID		AS MEMBER_ID      -- 會員編號
	 , item.ItemCode		AS MATNR		  -- 商品料號
from POSNEC.PosTradeM head
join POSNEC.PosTradeD item
  ON head.StoreID = item.StoreID
 and head.BusinessDate = item.BusinessDate
 and head.PosNo = item.PosNo
 and head.TradeSeqNo = item.TradeSeqNo
join POSNEC.POSDISCOUNTD discount
  ON item.StoreID = discount.StoreID
 and item.BusinessDate = discount.BusinessDate
 and item.PosNo = discount.PosNo
 and item.TradeSeqNo = discount.TradeSeqNo
 and item.ItemSeqNo = discount.ItemSeqNo
where head.TranArea not in ('200','201')</property>
