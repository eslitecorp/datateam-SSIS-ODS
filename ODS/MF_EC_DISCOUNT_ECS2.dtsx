              description="OLE DB 來源"
              name="select from Stage"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="命令逾時之前的秒數。值為 0 表示無限逾時。"
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="指定用於開啟資料列集之資料庫物件的名稱。"
                  name="OpenRowset">[SMS].[SMS]</property>
                <property
                  dataType="System.String"
                  description="指定包含用於開啟資料列集之資料庫物件名稱的變數。"
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="要執行的 SQL 命令。"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
     -- ** 交易基礎資訊PK+明細流水號+序號(EC沒有店日機)
	   concat(tmp.SUB_ORDER_NUMBER,tmp.Invoice_Number) as SL_KEY
     , tmp.ORDER_NUMBER AS TRANS_SEQNO		  -- 交易序號
	 , isnull(tmp.ITEM_SEQ_NO,1) as ITEM_SEQ_NO -- 明細流水號
	 -- ** 交易金額、數量 &amp; 相關欄位	
	 , tmp.SAP_BILLING_DATE AS TRANS_DATE   -- 交易時間
	 , tmp.FLAG
	 , tmp.INVOICE_QTY        AS QUANTITY   -- 數量  
	 , ISNULL(oi.QUOTEDPRICE,0) as UNIT_PRICE -- 單品原價
	 , tmp.SUBTOTAL_AMOUNT    as SALE_PRICE -- 單品售價
	 , tmp.SUBTOTAL_AMOUNT    as SALES_AMT  -- 含稅銷售金額
	 , (select sum(ISNULL(oi.ECOUPONDISCOUNT,0))
		from EC.O_OrderItems oi 
	   where '00' + tmp.ORDER_NUMBER = oi.OrderID 
	     and tmp.ARTICLE_REF_ID = oi.ProductID) as DSC_COUPON_AMT -- 贈券折抵金額
	 , tmp.SUBTAX_AMOUNT      AS TAX        -- 稅額
	   -- ** 商品資訊 &amp; 活動代碼欄位
	 , tmp.ARTICLE_ID	  AS MATNR      -- 商品 ID
	 , tmp.ARTICLE_DESC AS PROD_LNAME -- 產品長名
  from
(
select head.SUB_ORDER_NUMBER
     , head.Invoice_Number
	 , items.ORDER_NUMBER
	 , item_group.ITEM_SEQ_NO
	 , head.SAP_BILLING_DATE
	 , 1 AS FLAG
	 , items.INVOICE_QTY
	 , items.SUBTOTAL_AMOUNT
	 , items.ARTICLE_REF_ID
	 , items.SUBTAX_AMOUNT
	 , items.ARTICLE_ID
	 , items.ARTICLE_DESC
  from EC.ES_SAP_SUBORDERS head
  join EC.ES_SAP_SUBPRICED_ITEMS items 
    on head.SUB_ORDER_NUMBER = items.SUB_ORDER_NUMBER 
   and head.Invoice_Number = items.Invoice_Number
  left join (
	select SUB_ORDER_NUMBER,Invoice_Number,ARTICLE_ID,DENSE_RANK() OVER (PARTITION BY SUB_ORDER_NUMBER,Invoice_Number ORDER BY ARTICLE_ID) as ITEM_SEQ_NO
		from EC.ES_SAP_SUBPRICED_ITEMS
		group by SUB_ORDER_NUMBER,Invoice_Number,ARTICLE_ID
     ) as item_group
	on items.SUB_ORDER_NUMBER = item_group.SUB_ORDER_NUMBER 
   and items.Invoice_Number = item_group.Invoice_Number 
   and items.ARTICLE_ID = item_group.ARTICLE_ID
 where head.SAP_BILLING_DATE &gt;= concat(cast((YEAR(GETDATE()) - 5 + ?) as varchar),'0101')
 union 
select head.SUB_ORDER_NUMBER
     , head.Invoice_Number
	 , items.ORDER_NUMBER
	 , item_group.ITEM_SEQ_NO
	 , head.SAP_BILLING_DATE
	 , -1 AS FLAG
	 , items.INVOICE_QTY
	 , items.SUBTOTAL_AMOUNT
	 , items.ARTICLE_REF_ID
	 , items.SUBTAX_AMOUNT
	 , items.ARTICLE_ID
	 , items.ARTICLE_DESC
  from EC.ES_SAP_SUBORDERS_CANCEL head
  join EC.ES_SAP_SUBPRICED_ITEMS_CANCEL items 
    on head.SUB_ORDER_NUMBER = items.SUB_ORDER_NUMBER 
   and head.Invoice_Number = items.Invoice_Number
  left join (
	select SUB_ORDER_NUMBER,Invoice_Number,ARTICLE_ID,DENSE_RANK() OVER (PARTITION BY SUB_ORDER_NUMBER,Invoice_Number ORDER BY ARTICLE_ID) as ITEM_SEQ_NO
		from EC.ES_SAP_SUBPRICED_ITEMS_CANCEL
		group by SUB_ORDER_NUMBER,Invoice_Number,ARTICLE_ID
     ) as item_group
	on items.SUB_ORDER_NUMBER = item_group.SUB_ORDER_NUMBER 
   and items.Invoice_Number = item_group.Invoice_Number 
   and items.ARTICLE_ID = item_group.ARTICLE_ID
 where head.SAP_BILLING_DATE &gt;= concat(cast((YEAR(GETDATE()) - 5 + ?) as varchar),'0101')
     ) tmp
 outer apply(
	select top 1 ISNULL(oi.QUOTEDPRICE,0) as QUOTEDPRICE
	  from EC.O_OrderItems oi 
	 where '00' + tmp.ORDER_NUMBER = oi.OrderID and tmp.ARTICLE_REF_ID = oi.ProductID
  )oi</property>
