                catch (Exception ex)
                {
                    Row.ExtraInfo = ErrorColumnInfo;
                    Row.ErrorColumnName = "Unable to get Error Column Name";
                }
            }
        }
        catch (Exception ex)
        {
            Row.ExtraInfo = ex.ToString().Substring(0, 300);
            Row.ErrorColumnName = "Unable to get Error Column Name";
        }
    }

