            //'If an error occurred due to Check Constraint, the ErrorColumn would be 0, and that error affects the entire row. Hence there is no specific column for that error
            if (Row.ErrorColumn == 0)
            {
                Row.ErrorColumnName = "An error that affects the entire row";
            }
            //'If an error occurred due to Data type, then errorcolumn name would be updated.
            else
