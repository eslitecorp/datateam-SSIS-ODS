              <properties>
                <property
                  dataType="System.Int32"
                  description="命令逾時之前的秒數。值為 0 表示無限逾時。"
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="指定用於開啟資料列集之資料庫物件的名稱。"
                  name="OpenRowset">[SMS].[SMS]</property>
                <property
                  dataType="System.String"
                  description="指定包含用於開啟資料列集之資料庫物件名稱的變數。"
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="要執行的 SQL 命令。"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT CM.SN, MCH
		, PEC_LV.category_id, PEC_LV.category_name
		, PEC_LV.EC_L, PEC_LV.EC_L_Name
		, PEC_LV.EC_M, PEC_LV.EC_M_Name
		, PEC_LV.EC_S, PEC_LV.EC_S_Name
		, PEC_LV.level
	FROM EC.C_MCHConfig CM JOIN EC.P_EsliteCategory PEC ON  CAST(CM.CategoryID AS nvarchar(50)) = PEC.category_id
	JOIN (
		SELECT category_id as category_id, category_name as category_name
			, category_id as EC_L, category_name as EC_L_Name
			, null EC_M, null as EC_M_Name
			, null as EC_S, null as EC_S_Name
			, level
		FROM EC.P_EsliteCategory 
		WHERE level = 0
		 union
		--- LV1  153
		SELECT PELV1.category_id as category_id, PELV1.category_name as category_name
			, PELV0.category_id as EC_L, PELV0.category_name as EC_L_Name
			, PELV1.category_id EC_M, PELV1.category_name as EC_M_Name
			, null as EC_S, null as EC_S_Name
			, PELV1.level
		FROM (SELECT * FROM EC.P_EsliteCategory WHERE level = 1) PELV1
		JOIN EC.P_EsliteCategory PELV0 ON CAST(PELV1.parent_id AS nvarchar(50)) = PELV0.category_id AND PELV0.level = 0
		union
		--- LV2  644
		SELECT PELV2.category_id as category_id, PELV2.category_name as category_name
			, PELV0.category_id as EC_L, PELV0.category_name as EC_L_Name
			, PELV1.category_id EC_M, PELV1.category_name as EC_M_Name
			, PELV2.category_id as EC_S, PELV2.category_name as EC_S_Name
			, PELV2.level
		FROM (SELECT * FROM EC.P_EsliteCategory WHERE level = 2) PELV2
		JOIN EC.P_EsliteCategory PELV1 ON CAST(PELV2.parent_id AS nvarchar(50)) = PELV1.category_id AND PELV1.level = 1
		JOIN EC.P_EsliteCategory PELV0 ON CAST(PELV1.parent_id AS nvarchar(50)) = PELV0.category_id AND PELV0.level = 0
	) PEC_LV ON CAST(CM.CategoryID AS nvarchar(50)) = PEC_LV.category_id and PEC.level = PEC_LV.level
		--(CASE WHEN PEC.level = 0 THEN PEC_LV.EC_L WHEN PEC.level = 1 THEN PEC_LV.EC_M WHEN PEC.level = 2 THEN PEC_LV.EC_S END)
	WHERE LEN(MCH) = 9
	ORDER BY PEC.level;</property>
