                <component
                  refId="Package\Foreach 迴圈容器\Stage To ODS\select from Stage"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB 來源;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; 著作權所有，並保留一切權利; http://www.microsoft.com/sql/support;7"
                  description="OLE DB 來源"
                  name="select from Stage"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="命令逾時之前的秒數。值為 0 表示無限逾時。"
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="指定用於開啟資料列集之資料庫物件的名稱。"
                      name="OpenRowset">[SMS].[SMS]</property>
                    <property
                      dataType="System.String"
                      description="指定包含用於開啟資料列集之資料庫物件名稱的變數。"
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="要執行的 SQL 命令。"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
	 -- ** 交易基礎資訊PK+店日機序
	   head.PSN           AS SL_KEY       -- PK
     , head.STORE_NO      AS WERKS        -- 門市
	 , head.BUSINESS_DATE AS BUSINESSDATE -- 營業日期
	 , head.TM_NO         AS POSNO        -- 機號
	 , head.TRN_SEQ       AS TRANS_SEQNO  -- 序號
	 -- ** 交易金額 &amp; 相關欄位
	, case when concat(head.LEAD_CODE,head.TRN_TYPE) in ('T211','T511') THEN '1' ELSE head.FLAG END AS FLAG -- 正/負向交易
	 , case when concat(head.LEAD_CODE,head.TRN_TYPE) in ('T211','T511') THEN  '100'
	        when concat(head.LEAD_CODE,head.TRN_TYPE) in ('T219','T519') THEN  '110'
			when concat(head.LEAD_CODE,head.TRN_TYPE) in ('T221','T521') THEN  '102'
			else '' END AS TRANS_TYPE     -- 交易別
	 , head.TM_DATE -- ETL組合TRANS_DATE
	 , head.TM_TIME -- ETL組合TRANS_DATE
	 , head.INV_KIND    AS INV_TYPE       -- 發票別
	 , head.INV_NO      AS INVOICE_NO     -- 發票號碼
     , head.INV_AMT     AS INVOICE_AMT    -- 發票金額
	 , head.TRN_AMT     AS SALES_AMT      -- 銷售總額(含稅)
	 , isnull(head.TRN_AMT,0) - isnull(head.TAX,0) AS SALES_AMT_NOTAX  -- 交易金額(未稅)
	 , head.DISCOUNT    AS TOT_DISC_AMT   -- 總額讓額
	 , head.TAX                           -- 稅額
	 , head.TAX_TOT_AMT                   -- 應稅總金額
	 , head.NONTAX_TOT_AMT                -- 免稅總金額
	 , head.ART_QTY                       -- 品項總數量
	 , head.SALE_UNIT   AS MSM_ORDER      -- MSM客訂單號
	 -- ** 退貨資訊
	 , case when concat(head.LEAD_CODE,head.TRN_TYPE) in ('T221','T521') 
	        THEN BUSINESS_DATE
			ELSE ''
			END AS RTN_DATE               -- 退貨日期
	 , case when concat(head.LEAD_CODE,head.TRN_TYPE) in ('T221','T521') 
			THEN (select RTN_INFO 
			        from POS.TRN_HEAD_INIT 
				   where BUSINESS_DATE &lt;= head.BUSINESS_DATE 
				     and STORE_NO = head.STORE_NO 
					 and TM_NO = head.INVALID_TM 
					 and TRN_SEQ = head.INVALID_NO 
					 and INVALID_TM = head.TM_NO 
					 and INVALID_NO = head.TRN_SEQ
					 and TRN_AMT = head.TRN_AMT) 
			ELSE '' END AS RTN_INFO       -- 原始交易主鍵,歸結日期，再用ETL分隔
	 -- ** 地點
	 , head.MONOPOLY_NO AS LIFNR_COUNTER
