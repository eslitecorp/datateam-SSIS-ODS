            <component
              refId="Package\Stage To ODS\select from Stage"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB 來源;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; 著作權所有，並保留一切權利; http://www.microsoft.com/sql/support;7"
              description="OLE DB 來源"
              name="select from Stage"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="命令逾時之前的秒數。值為 0 表示無限逾時。"
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="指定用於開啟資料列集之資料庫物件的名稱。"
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="指定包含用於開啟資料列集之資料庫物件名稱的變數。"
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="要執行的 SQL 命令。"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
	 -- ** 交易基礎資訊PK+店日機序
	   concat(head.StoreID,replace(head.BusinessDate,'/',''),PosNo,TradeSeqNo) AS SL_KEY -- PK
     , head.StoreID       AS WERKS        -- 門市
	 , head.BusinessDate  AS BUSINESSDATE -- 營業日期
	 , head.PosNo         AS POSNO        -- 機號
	 , head.TradeSeqNo    AS TRANS_SEQNO  -- 序號
	 -- ** 交易金額 &amp; 相關欄位
	 , case when head.TranArea in ('102','103','110') 
			THEN -1 ELSE 1 END AS FLAG        -- 正/負向交易
	 , head.TranArea        AS TRANS_TYPE     -- 交易別
	 , head.TranSubArea     AS TRANS_SUBTYPE  -- 交易子別
	 , head.TradeTime -- ETL組合TRANS_DATE/INVOICE_DATE/ORDER_DATE
	 , head.InvoiceType     AS INV_TYPE       -- 發票別
	 , head.InvoiceNo       AS INVOICE_NO     -- 發票號碼
     , head.InvoiceAmt      AS INVOICE_AMT    -- 發票金額
	 , head.SalesAmt        AS SALES_AMT      -- 銷售總額(含稅)
	 , isnull(SalesAmt,0) - isnull(SalesTaxAmt,0)  AS SALES_AMT_NOTAX -- 交易金額(未稅)
	 , head.DiscountAmt     AS TOT_DISC_AMT     -- 總額讓額
	 , head.SalesTaxAmt     AS TAX            -- 稅額
	 , head.InTaxSalesAmt   AS TAX_TOT_AMT    -- 應稅總金額
	 , head.FreeTaxSalesAmt AS NONTAX_TOT_AMT -- 免稅總金額
	 , head.SalesQty        AS ART_QTY        -- 品項總數量
	 , head.MsmOrderNo  AS MSM_ORDER          -- MSM客訂單號
	 -- ** 退貨資訊
	 , case when head.TranArea in ('102','103','110') 
	        THEN head.BusinessDate
			ELSE ''
			END AS RTN_DATE               -- 退貨日期
	 , case when head.TranArea in ('102','103','110') 
			THEN (select concat(head.StoreID,replace(head.BusinessDate,'/',''),PosNo,TradeSeqNo)+','+TradeTime
			        from POSNEC.PosTradeM 
				   where StoreID = head.StoreID 
					 and BusinessDate = head.VoidBusinessDate 
					 and PosNo = head.VoidPosNo 
					 and TradeSeqNo = head.VoidTradeSeqNo ) 
			ELSE '' END AS RTN_INFO       -- 原始交易主鍵,歸結日期，再用ETL分隔
