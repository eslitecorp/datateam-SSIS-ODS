      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT CONCAT(HEAD.STOREID,REPLACE(HEAD.BUSINESSDATE,'/',''),HEAD.POSNO,HEAD.TRADESEQNO) AS SL_KEY&#xA;     , HEAD.STOREID &#x9;&#x9;&#x9;AS WERK&#xA;&#x9; , HEAD.BUSINESSDATE &#x9;&#x9;AS BUSINESSDATE&#xA;&#x9; , HEAD.POSNO &#x9;&#x9;&#x9;&#x9;AS POSNO&#xA;&#x9; ,HEAD.TRADESEQNO &#x9;&#x9;AS TRANS_SEQNO&#xA;&#x9; , HEAD.TRANAREA &#x9;&#x9;&#x9;AS TRANS_TYPE&#xA;&#x9; , HEAD.TRANSUBAREA &#x9;&#x9;AS TRANS_SUBTYPE&#xA;&#x9; , HEAD.TRADETIME &#x9;&#x9;&#x9;AS TRANS_DATE&#xA;&#x9; , HEAD.TRADETIME &#x9;&#x9;&#x9;AS INVOICE_DATE&#xA;&#x9; /*, CASE WHEN PAY.PAYMENTID = '31' THEN PAY.PAYMENTAMT ELSE 0 END AS DSC_POINT_AMT --改 DM 執行 */&#xA;&#x9; /*, CASE WHEN PAY.PAYMENTID = '10' THEN PAY.PAYMENTAMT ELSE 0 END AS DSC_COUPON_AMT --改 DM 執行 */&#xA;&#x9; , HEAD.TRADETIME &#x9;&#x9;&#x9;AS ORDER_DATE&#xA;&#x9; , HEAD.TENANTNO &#x9;&#x9;&#x9;AS LIFNR_COUNTER&#xA;&#x9; , HEAD.BUILDINGID &#x9;&#x9;&#x9;AS BUILDING_ID&#xA;&#x9; , HEAD.FLOOR &#x9;&#x9;&#x9;&#x9;AS FLOOR&#xA;&#x9; , HEAD.STORENM&#x9;&#x9;&#x9;&#x9;AS WERKS_SNAME&#xA;&#x9; , HEAD.INVOICETYPE&#x9;&#x9;&#x9;AS INV_TYPE&#xA;&#x9; , HEAD.INVOICENO&#x9;&#x9;&#x9;AS INVOICE_NO&#xA;&#x9; , HEAD.INVOICEAMT&#x9;&#x9;&#x9;AS INVOICE_AMT&#xA;&#x9; , HEAD.MEMBERNO&#x9;&#x9;&#x9;AS CARD_ID&#xA;&#x9; , HEAD.MEMBERCARDTYPE  &#x9;AS MEMBER_EN&#xA;&#x9; , HEAD.MEMBERDISCRANK  &#x9;AS MEMBER_EN_PARENT&#xA;&#x9; , HEAD.MSMORDERNO      &#x9;AS MSM_ORDER&#xA;&#x9; , CASE WHEN HEAD.TRANAREA IN ('102', '103', '110')&#xA;            THEN HEAD.VoidBusinessDate&#xA;       END                      AS RTN_DATE&#xA;&#x9; , HEAD.TRADETIME&#x9;&#x9;&#x9;AS RTN_BILLING_DATE&#xA;&#x9; , CASE WHEN HEAD.TRANAREA IN ('102','103','110') &#xA;&#x9;&#x9;&#x9;THEN (SELECT CONCAT(HEAD.STOREID,REPLACE(BUSINESSDATE,'/',''),POSNO,TRADESEQNO)&#xA;&#x9;&#x9;&#x9;        FROM STAGINGDB_MY.TPOS.POSTRADEM &#xA;&#x9;&#x9;&#x9;&#x9;   WHERE STOREID &#x9;&#x9;= HEAD.STOREID &#xA;&#x9;&#x9;&#x9;&#x9;&#x9; AND BUSINESSDATE &#x9;= HEAD.VOIDBUSINESSDATE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9; AND POSNO &#x9;&#x9;&#x9;= HEAD.VOIDPOSNO &#xA;&#x9;&#x9;&#x9;&#x9;&#x9; AND TRADESEQNO &#x9;= HEAD.VOIDTRADESEQNO ) &#xA;&#x9;&#x9;&#x9;ELSE '' END &#x9;&#x9;AS SL_KEY_O&#xA;&#x9; , 'TPOS'&#x9;&#x9;&#x9;&#x9;&#x9;AS SRC_SYS&#xA;&#x9; , CASE WHEN HEAD.TRANAREA IN ('102', '103', '110')&#xA;            THEN -1 ELSE 1 END  AS FLAG&#xA;&#x9; , HEAD.SALESTAXAMT&#x9;&#x9;&#x9;AS TAX&#xA;&#x9; , HEAD.INTAXSALESAMT&#x9;&#x9;AS TAX_TOT_AMT&#xA;&#x9; , HEAD.FREETAXSALESAMT &#x9;AS NONTAX_TOT_AMT&#xA;&#x9; , HEAD.SALESQTY&#x9;&#x9;&#x9;AS ART_QTY&#xA;&#x9; , HEAD.SALESAMT&#x9;&#x9;&#x9;AS SALES_AMT&#xA;&#x9; , HEAD.SALESAMT - HEAD.SALESTAXAMT&#x9;&#x9;AS SALES_AMT_NOTAX &#xA;&#x9; , HEAD.DISCOUNTAMT&#x9;&#x9;&#x9;AS TOT_DISC_AMT&#xA;&#x9; , HEAD.BUYERID&#x9;&#x9;&#x9;&#x9;AS VAT_NO&#xA;&#x9; , HEAD.MEMBERPID&#x9;&#x9;&#x9;AS MEMBER_ID&#xA;&#x9; , HEAD.EMPNO&#x9;&#x9;&#x9;&#x9;AS EMPNO&#xA;  FROM STAGINGDB_MY.TPOS.POSTRADEM HEAD&#xA;  &#xA;  /* 改由 DM 執行&#xA;  LEFT JOIN (SELECT STOREID , BUSINESSDATE , POSNO , TRADESEQNO , PAYMENTID ,  SUM(PAYMENTAMT) AS PAYMENTAMT&#xA;&#x9;&#x9;&#x9;   FROM STAGINGDB_MY.TPOS.POSPAYMENT &#xA;&#x9;&#x9;&#x9;  GROUP BY STOREID , BUSINESSDATE , POSNO , TRADESEQNO , PAYMENTID) PAY    &#xA;&#x9;ON HEAD.STOREID      = PAY.STOREID&#xA;   AND HEAD.BUSINESSDATE = PAY.BUSINESSDATE&#xA;   AND HEAD.POSNO        = PAY.POSNO&#xA;   AND HEAD.TRADESEQNO   = PAY.TRADESEQNO&#xA;   */&#xA;   &#xA; WHERE HEAD.TRANAREA NOT IN ('200','201')&#xA; and (head.ETL_DATE &gt;= cast(cast('&quot; + ((@[User::ReloadWorkDate]==&quot;&quot;) ? (DT_STR, 50, 950) GETDATE()  : @[User::ReloadWorkDate]) +&quot;' as date) as datetime))&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="queryFromStage">
      <DTS:VariableValue
        DTS:DataType="8">SELECT CONCAT(HEAD.STOREID,REPLACE(HEAD.BUSINESSDATE,'/',''),HEAD.POSNO,HEAD.TRADESEQNO) AS SL_KEY
     , HEAD.STOREID 			AS WERK
	 , HEAD.BUSINESSDATE 		AS BUSINESSDATE
	 , HEAD.POSNO 				AS POSNO
	 ,HEAD.TRADESEQNO 		AS TRANS_SEQNO
	 , HEAD.TRANAREA 			AS TRANS_TYPE
	 , HEAD.TRANSUBAREA 		AS TRANS_SUBTYPE
	 , HEAD.TRADETIME 			AS TRANS_DATE
	 , HEAD.TRADETIME 			AS INVOICE_DATE
	 /*, CASE WHEN PAY.PAYMENTID = '31' THEN PAY.PAYMENTAMT ELSE 0 END AS DSC_POINT_AMT --改 DM 執行 */
	 /*, CASE WHEN PAY.PAYMENTID = '10' THEN PAY.PAYMENTAMT ELSE 0 END AS DSC_COUPON_AMT --改 DM 執行 */
	 , HEAD.TRADETIME 			AS ORDER_DATE
	 , HEAD.TENANTNO 			AS LIFNR_COUNTER
	 , HEAD.BUILDINGID 			AS BUILDING_ID
	 , HEAD.FLOOR 				AS FLOOR
	 , HEAD.STORENM				AS WERKS_SNAME
	 , HEAD.INVOICETYPE			AS INV_TYPE
	 , HEAD.INVOICENO			AS INVOICE_NO
	 , HEAD.INVOICEAMT			AS INVOICE_AMT
	 , HEAD.MEMBERNO			AS CARD_ID
	 , HEAD.MEMBERCARDTYPE  	AS MEMBER_EN
	 , HEAD.MEMBERDISCRANK  	AS MEMBER_EN_PARENT
	 , HEAD.MSMORDERNO      	AS MSM_ORDER
	 , CASE WHEN HEAD.TRANAREA IN ('102', '103', '110')
            THEN HEAD.VoidBusinessDate
       END                      AS RTN_DATE
	 , HEAD.TRADETIME			AS RTN_BILLING_DATE
	 , CASE WHEN HEAD.TRANAREA IN ('102','103','110') 
			THEN (SELECT CONCAT(HEAD.STOREID,REPLACE(BUSINESSDATE,'/',''),POSNO,TRADESEQNO)
			        FROM STAGINGDB_MY.TPOS.POSTRADEM 
				   WHERE STOREID 		= HEAD.STOREID 
					 AND BUSINESSDATE 	= HEAD.VOIDBUSINESSDATE 
					 AND POSNO 			= HEAD.VOIDPOSNO 
					 AND TRADESEQNO 	= HEAD.VOIDTRADESEQNO ) 
			ELSE '' END 		AS SL_KEY_O
	 , 'TPOS'					AS SRC_SYS
	 , CASE WHEN HEAD.TRANAREA IN ('102', '103', '110')
            THEN -1 ELSE 1 END  AS FLAG
	 , HEAD.SALESTAXAMT			AS TAX
	 , HEAD.INTAXSALESAMT		AS TAX_TOT_AMT
	 , HEAD.FREETAXSALESAMT 	AS NONTAX_TOT_AMT
	 , HEAD.SALESQTY			AS ART_QTY
	 , HEAD.SALESAMT			AS SALES_AMT
	 , HEAD.SALESAMT - HEAD.SALESTAXAMT		AS SALES_AMT_NOTAX 
	 , HEAD.DISCOUNTAMT			AS TOT_DISC_AMT
	 , HEAD.BUYERID				AS VAT_NO
	 , HEAD.MEMBERPID			AS MEMBER_ID
	 , HEAD.EMPNO				AS EMPNO
  FROM STAGINGDB_MY.TPOS.POSTRADEM HEAD
  
  /* 改由 DM 執行
  LEFT JOIN (SELECT STOREID , BUSINESSDATE , POSNO , TRADESEQNO , PAYMENTID ,  SUM(PAYMENTAMT) AS PAYMENTAMT
			   FROM STAGINGDB_MY.TPOS.POSPAYMENT 
			  GROUP BY STOREID , BUSINESSDATE , POSNO , TRADESEQNO , PAYMENTID) PAY    
	ON HEAD.STOREID      = PAY.STOREID
   AND HEAD.BUSINESSDATE = PAY.BUSINESSDATE
   AND HEAD.POSNO        = PAY.POSNO
   AND HEAD.TRADESEQNO   = PAY.TRADESEQNO
   */
   
 WHERE HEAD.TRANAREA NOT IN ('200','201')
 and (head.ETL_DATE &gt;= cast(cast('2023-01-11 14:09:23.542000000' as date) as datetime))</DTS:VariableValue>
    </DTS:Variable>
