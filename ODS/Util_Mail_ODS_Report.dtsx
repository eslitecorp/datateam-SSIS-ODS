        /// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {
            // excel full path
            string reportFilePath = Dts.Variables["User::rptFilePath_WerksTrans"].Value.ToString();

            OleDbDataAdapter adapter = new OleDbDataAdapter();
            DataTable resultSet_List = new DataTable();
            adapter.Fill(resultSet_List, Dts.Variables["User::result_WerksTrans_List"].Value);
            try
            {
                // =================================================================
                // #######  依照查詢結果清單，彙整錯誤與成功門市統計  #######  
                System.Collections.Generic.HashSet<String> errSet = new System.Collections.Generic.HashSet<String>();
                System.Collections.Generic.HashSet<String> passSet = new System.Collections.Generic.HashSet<String>();
                foreach (DataRow dr in resultSet_List.Rows)
                {
                    // 檢查交易資料是否有未回傳情形
                    // 1. YES
                    if (Int32.Parse(dr[5].ToString()) + Int32.Parse(dr[6].ToString()) == 0)
                    {
                        // 1-1. 檢查有沒有被放進去pass清單中，如果有，從pass清單中移除
                        if (passSet.Contains(dr[0].ToString()))
                        {
                            passSet.Remove(dr[0].ToString());
                        }
                        // 1-2. 加入錯誤清單
                        if (!errSet.Contains(dr[0].ToString()))
                        {
                            errSet.Add(dr[0].ToString());
                        }
                    }
                    else
                    {
                        if (!passSet.Contains(dr[0].ToString()))
                        {
                            passSet.Add(dr[0].ToString());
                        }
                    }
                }
                DataTable resultSet_Stat = new DataTable();
                DataColumn column;
                // Create new DataColumn, set DataType, ColumnName and add to DataTable.    
                column = new DataColumn();
                column.DataType = System.Type.GetType("System.Int32");
                column.ColumnName = "errAmt";
                resultSet_Stat.Columns.Add(column);

                column = new DataColumn();
                column.DataType = Type.GetType("System.Int32");
                column.ColumnName = "passAmt";
                resultSet_Stat.Columns.Add(column);

                DataRow statDr = resultSet_Stat.NewRow();
                statDr["errAmt"] = errSet.Count;
                statDr["passAmt"] = passSet.Count;
                resultSet_Stat.Rows.Add(statDr);
                // =================================================================
                // #######  產製EXCEL  #######  
                this.fixExcelStyle(reportFilePath, 0, 4, resultSet_Stat,0); // execution stat
                this.fixExcelStyle(reportFilePath, 0, 7, resultSet_List,1); // execution list
                // =================================================================
                // #######  產製mail  #######  
                int errAmt = Int32.Parse(resultSet_Stat.Rows[0]["errAmt"].ToString());
                // 異常或是未執行，對應的mail主旨與內文處理
                String subject = "「ODS」資料處理 門市交易寫入執行完成" + "_" + Dts.Variables["User::workDate"].Value.ToString().Replace("-", "");
                StringBuilder mailMsg = new StringBuilder();
                if (errAmt > 0)
                {
                    // mail 主旨處理
                    subject = "「ODS」資料處理 門市交易缺少" + "_" + Dts.Variables["User::workDate"].Value.ToString().Replace("-", "");

                    //// mail 內文處理
                    foreach (DataRow detailDr in resultSet_List.Rows)
                    {
                        if (Int32.Parse(detailDr[5].ToString()) + Int32.Parse(detailDr[6].ToString()) == 0)
                        {
                            mailMsg.Append(detailDr[0].ToString() + " " + detailDr[1].ToString() + " 於 " + detailDr[4].ToString() + " 交易資料缺少\r\n");
                        }
                    }
                }
                else
                {
                    mailMsg.Append("近7日門市交易寫入執行完成");
                }
                Dts.Variables["User::mailSubject_WerksTrans"].Value = subject;
                Dts.Variables["User::mailMsg_WerksTrans"].Value = mailMsg.ToString();
            }
            catch (Exception ex)
            {
                Dts.Log(ex.Message, 999, null);
            }
            Dts.TaskResult = (int)ScriptResults.Success;
        }

        public void fixExcelStyle(String excelPath, int workSheetIndex, int startRowIdx, DataTable dataTable, int type)
        {
            try
            {
                XSSFWorkbook wb;
                using (FileStream fs = new FileStream(excelPath, FileMode.Open, FileAccess.ReadWrite))
                {
                    wb = new XSSFWorkbook(fs);

                    ISheet ws = wb.GetSheetAt(workSheetIndex);

                    // Header處理：ETL作業執行日期(WORK_DATE)
                    if (type == 0)
                    {
                        ws.GetRow(1).GetCell(0).SetCellValue("ETL作業執行日期(WORK_DATE)：" + Dts.Variables["User::workDate"].Value.ToString());
                    }

                    // base format 處理
                    IFont font = wb.CreateFont();
                    font.FontName = "微軟正黑體";
                    font.FontHeightInPoints = 8;

                    // 將每一筆查詢結果寫入excel
                    IRow baseRow = ws.GetRow(startRowIdx);
                    foreach (DataRow dr in dataTable.Rows)
                    {
                        // 取得excel資料列
                        IRow row = ws.GetRow(startRowIdx);
                        if (row == null)
                        {
                            row = ws.CreateRow(startRowIdx);
                        }
                        setRowDataByType(baseRow, row, dr, type, font);
                        startRowIdx++;
                    }

                    // 設定自動欄位寬
                    for (int cellNo = 0; cellNo <= 6; cellNo++)
                    {
                        ws.AutoSizeColumn(cellNo);
                    }

                    // 寫入excel & close file
                    using (FileStream fs2 = new FileStream(excelPath, FileMode.Create, FileAccess.Write))
                    {
                        wb.Write(fs2);
                        fs2.Close();
                        fs2.Dispose();
                    }

                    fs.Close();
                    fs.Dispose();
                }

                wb.Close();
            }
            catch (Exception ex)
            {
                throw ex;
            }
        }

        public void setRowDataByType(IRow baseRow, IRow row, DataRow dr, int type, IFont font)
        {
            if (type == 0) // 統計的數值填入excel
            {
                // 錯誤
                ICell cell = setCellFormat(row, 2, null);
                cell.SetCellValue(Int32.Parse(dr["errAmt"].ToString()));
                // 成功
                ICell cell2 = setCellFormat(row, 4, null);
                cell2.SetCellValue(Int32.Parse(dr["passAmt"].ToString()));
            }
            else if (type == 1) // 執行狀態清單
            {
                //標示紅色
                Boolean amtErr = false;
                if (dr[5].ToString() == "0" && dr[6].ToString() == "0")
                {
                    amtErr = true;
                }

                // 依照查詢SQL中，column順序設定到對應excel (!!!順序需一致!!!)
                for (int cellNo = 0; cellNo <= 6; cellNo++)
                {
                    //// 設定欄位字體紅色
                    //if (amtErr && (cellNo == 4 || cellNo == 5 || cellNo == 6))
                    //{
                    //    font.Color = NPOI.SS.UserModel.IndexedColors.Red.Index;
                    //}

                    ICell cell = setCellFormat(row, cellNo, font);
                    ICell baseCell = baseRow.GetCell(cellNo);
                    try
                    {
                        if (baseCell != null)
                        {
                            cell.SetCellType(baseCell.CellType); //複製第一個資料列資料型態
                        }

                        // 設定欄位數值/文字格式
                        if (cellNo == 5 || cellNo == 6)
                        {
                            // 數值格式欄位：[5、6]筆數
                            int v = Int32.Parse(dr[cellNo].ToString());
                            cell.SetCellValue(v);
                        }
                        else
                        {
                            cell.SetCellValue(dr[cellNo].ToString());
                        }


                    }
                    catch (Exception)
                    {
                    }
                }
            }
        }

        public ICell setCellFormat(IRow row, int colInx, IFont font)
        {
            ICell cell = row.GetCell(colInx);
            if (cell == null)
            {
                cell = row.CreateCell(colInx);
            }
            if (font != null)
            {
                ICellStyle cloneStyle = cell.CellStyle;
                cloneStyle.VerticalAlignment = VerticalAlignment.Center;
                cloneStyle.Alignment = NPOI.SS.UserModel.HorizontalAlignment.Left;
                cloneStyle.SetFont(font);
                cell.CellStyle.WrapText = true;
            }
            return cell;
        }

