    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{5E670877-B584-466A-908E-61D62F23BEF8}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;SELECT DG, WORK_DATE, AVG(GOD) AS GOD, GID, COUNT(*) AS TAB_NUMS, &#xA;&#x9;&#x9;&#x9;SUM(CASE WHEN FILE_NUMS = FILE_NUMS_COMP THEN 1 ELSE 0 END) AS TAB_NUMS_COMP, &#xA;&#x9;&#x9;&#x9;SUM(FILE_NUMS) AS FILE_NUMS, SUM(FILE_NUMS_COMP) AS FILE_NUMS_COMP, &#xA;&#x9;&#x9;&#x9;SUM(FILE_ROWS) AS FILE_ROWS, SUM(FILE_ROWS_COMP) AS FILE_ROWS_COMP, &#xA;&#x9;&#x9;&#x9;MIN(STATUS) AS STATUS, MIN(ODS_JOBID) AS ODS_JOBID, &#xA;&#x9;&#x9;&#x9;MIN(ODS_STATUS) AS ODS_STATUS, MIN(ODS_MSG) AS ODS_MSG, MIN(ODS_PDATE) AS ODS_PDATE, &#xA;&#x9;&#x9;&#x9;MIN(ODS_SDATE) AS ODS_SDATE, MIN(ODS_EDATE) AS ODS_EDATE, MIN(ODS_PID) AS ODS_PID&#xA;&#x9;　FROM  (SELECT　A.SYS_NAME, A.EKORG, A.WORK_DATE, A.TABLE_NAME, A.GID, A.DG, A.GOD, B.FILE_NUMS, B.FILE_NUMS_COMP, &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;　B.FILE_ROWS, B.FILE_ROWS_COMP, A.STATUS, A.ODS_JOBID, A.ODS_STATUS, A.ODS_MSG, A.ODS_PDATE, &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;　A.ODS_SDATE, A.ODS_EDATE, A.ODS_PID&#xA;&#x9;&#x9;&#x9;　　FROM  (select * from dbo.STAGE_TAB_ACT union select * from dbo.STAGE_TAB_HIS) AS A &#xA;&#x9;&#x9;&#x9;　　LEFT OUTER JOIN　dbo.V_FILE_INFO AS B ON A.SYS_NAME = B.SYS_NAME AND A.EKORG = B.EKORG AND &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;A.TABLE_NAME = B.TABLE_NAME AND A.WORK_DATE = B.WORK_DATE&#xA;&#x9;&#x9;&#x9;&#x9;WHERE  (A.WORK_DATE IS NOT NULL)&#xA;&#x9;&#x9;&#x9;) AS A&#xA;&#x9;　GROUP BY DG, GID, WORK_DATE&#xA;) stg&#xA;left join ODSDB.dbo.ETL_EXECUTION_LOG ods on stg.DG = ods.DG and stg.GID = ods.GID and stg.WORK_DATE = ods.WORK_DATE&#xA;where stg.DG &lt;&gt; 'NULL' and stg.WORK_DATE = '&quot;+ @[User::workDate] +&quot;'&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="sqlFlow_Base">
      <DTS:VariableValue
        DTS:DataType="8">SELECT DG, WORK_DATE, AVG(GOD) AS GOD, GID, COUNT(*) AS TAB_NUMS, 
			SUM(CASE WHEN FILE_NUMS = FILE_NUMS_COMP THEN 1 ELSE 0 END) AS TAB_NUMS_COMP, 
			SUM(FILE_NUMS) AS FILE_NUMS, SUM(FILE_NUMS_COMP) AS FILE_NUMS_COMP, 
			SUM(FILE_ROWS) AS FILE_ROWS, SUM(FILE_ROWS_COMP) AS FILE_ROWS_COMP, 
			MIN(STATUS) AS STATUS, MIN(ODS_JOBID) AS ODS_JOBID, 
			MIN(ODS_STATUS) AS ODS_STATUS, MIN(ODS_MSG) AS ODS_MSG, MIN(ODS_PDATE) AS ODS_PDATE, 
			MIN(ODS_SDATE) AS ODS_SDATE, MIN(ODS_EDATE) AS ODS_EDATE, MIN(ODS_PID) AS ODS_PID
	　FROM  (SELECT　A.SYS_NAME, A.EKORG, A.WORK_DATE, A.TABLE_NAME, A.GID, A.DG, A.GOD, B.FILE_NUMS, B.FILE_NUMS_COMP, 
					　B.FILE_ROWS, B.FILE_ROWS_COMP, A.STATUS, A.ODS_JOBID, A.ODS_STATUS, A.ODS_MSG, A.ODS_PDATE, 
					　A.ODS_SDATE, A.ODS_EDATE, A.ODS_PID
			　　FROM  (select * from dbo.STAGE_TAB_ACT union select * from dbo.STAGE_TAB_HIS) AS A 
			　　LEFT OUTER JOIN　dbo.V_FILE_INFO AS B ON A.SYS_NAME = B.SYS_NAME AND A.EKORG = B.EKORG AND 
								A.TABLE_NAME = B.TABLE_NAME AND A.WORK_DATE = B.WORK_DATE
				WHERE  (A.WORK_DATE IS NOT NULL)
			) AS A
	　GROUP BY DG, GID, WORK_DATE
) stg
left join ODSDB.dbo.ETL_EXECUTION_LOG ods on stg.DG = ods.DG and stg.GID = ods.GID and stg.WORK_DATE = ods.WORK_DATE
where stg.DG &lt;&gt; 'NULL' and stg.WORK_DATE = '2020-03-16'</DTS:VariableValue>
    </DTS:Variable>
    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{48D2AE8B-B840-4231-A6CC-4A74AF788C8E}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;select stg.DG&#xA;     , stg.GID&#xA;&#x9; , stg.GOD&#xA;&#x9; , case when isnull(ods.PACKAGE_NAME,'') &lt;&gt; '' then concat(ods.PACKAGE_NAME,'.dtsx') else '' end as  pkgName&#xA;&#xA;&#x9; , case when stg.ODS_STATUS = 2 then isnull(ods.EXECUTION_ID,0)&#xA;&#xA;&#x9;        when stg.ODS_STATUS = 3 then (&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;select left(value,CHARINDEX('，執行狀態',value)-1)&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;from STRING_SPLIT(REPLACE(stg.ODS_MSG,'執行識別碼: ','［'), '［')&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;where value like '%執行狀態%'&#xA;&#xA;&#x9;&#x9;&#x9;)&#xA;&#xA;&#x9;   else 0 end as EXECUTION_ID&#xA;&#xA;&#x9; , case stg.ODS_STATUS when 2 then '完成' when 3 then '異常' else '未執行' end as status&#xA;&#xA;&#x9; , isnull(ods.QUERY_ROW_COUNT,0) as QUERY_ROW_COUNT&#xA;&#xA;&#x9; , isnull(ods.DEL_ROW_COUNT,0) as DEL_ROW_COUNT&#xA;&#xA;&#x9; , isnull(ods.UPSERT_ROW_COUNT,0) as UPSERT_ROW_COUNT&#xA;&#xA;&#x9; , case when stg.ODS_STATUS in (2,3) then convert(varchar, stg.ODS_PDATE, 121) else '' end as sTime&#xA;&#xA;&#x9; , ISNULL(DATEDIFF(ss,stg.ODS_PDATE,stg.ODS_EDATE),0) as 'duration(sec)'&#xA;&#x9; --, case when stg.ODS_STATUS = 3 then (&#xA;&#x9;&#x9;--&#x9;&#x9;SELECT STRING_AGG(message,'；') as msg&#xA;&#x9;&#x9;--&#x9;&#x9;  FROM [SSISDB].[catalog].[operation_messages]&#xA;&#x9;&#x9;--&#x9;&#x9;  where operation_id = (&#xA;&#x9;&#x9;--&#x9;&#x9;&#x9;&#x9;&#x9;select left(value,CHARINDEX('，執行狀態',value)-1)&#xA;&#x9;&#x9;--&#x9;&#x9;&#x9;&#x9;&#x9;from STRING_SPLIT(REPLACE(stg.ODS_MSG,'執行識別碼: ','［'), '［')&#xA;&#x9;&#x9;--&#x9;&#x9;&#x9;&#x9;&#x9;where value like '%執行狀態%'&#xA;&#x9;&#x9;--&#x9;&#x9;&#x9;)　&#xA;&#x9;&#x9;--&#x9;&#x9;   and message_type = 120&#xA;&#x9;&#x9;--&#x9;)&#xA;&#x9;&#x9;--&#x9;else '' end as errorMsg&#xA;from &#xA;(&#xA;&quot; + @[User::sqlFlow_Base]   + &quot;&#xA;order by isnull(stg.ODS_STATUS,0) desc, DG, GOD desc&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="sqlFlow_List">
      <DTS:VariableValue
        DTS:DataType="8">select stg.DG
     , stg.GID
	 , stg.GOD
	 , case when isnull(ods.PACKAGE_NAME,'') &lt;&gt; '' then concat(ods.PACKAGE_NAME,'.dtsx') else '' end as  pkgName

	 , case when stg.ODS_STATUS = 2 then isnull(ods.EXECUTION_ID,0)

	        when stg.ODS_STATUS = 3 then (

					select left(value,CHARINDEX('，執行狀態',value)-1)

					from STRING_SPLIT(REPLACE(stg.ODS_MSG,'執行識別碼: ','［'), '［')

					where value like '%執行狀態%'

			)

	   else 0 end as EXECUTION_ID

	 , case stg.ODS_STATUS when 2 then '完成' when 3 then '異常' else '未執行' end as status

	 , isnull(ods.QUERY_ROW_COUNT,0) as QUERY_ROW_COUNT

	 , isnull(ods.DEL_ROW_COUNT,0) as DEL_ROW_COUNT

	 , isnull(ods.UPSERT_ROW_COUNT,0) as UPSERT_ROW_COUNT

	 , case when stg.ODS_STATUS in (2,3) then convert(varchar, stg.ODS_PDATE, 121) else '' end as sTime

	 , ISNULL(DATEDIFF(ss,stg.ODS_PDATE,stg.ODS_EDATE),0) as 'duration(sec)'
	 --, case when stg.ODS_STATUS = 3 then (
		--		SELECT STRING_AGG(message,'；') as msg
		--		  FROM [SSISDB].[catalog].[operation_messages]
		--		  where operation_id = (
		--					select left(value,CHARINDEX('，執行狀態',value)-1)
		--					from STRING_SPLIT(REPLACE(stg.ODS_MSG,'執行識別碼: ','［'), '［')
		--					where value like '%執行狀態%'
		--			)　
		--		   and message_type = 120
		--	)
		--	else '' end as errorMsg
from 
(
SELECT DG, WORK_DATE, AVG(GOD) AS GOD, GID, COUNT(*) AS TAB_NUMS, 
			SUM(CASE WHEN FILE_NUMS = FILE_NUMS_COMP THEN 1 ELSE 0 END) AS TAB_NUMS_COMP, 
			SUM(FILE_NUMS) AS FILE_NUMS, SUM(FILE_NUMS_COMP) AS FILE_NUMS_COMP, 
			SUM(FILE_ROWS) AS FILE_ROWS, SUM(FILE_ROWS_COMP) AS FILE_ROWS_COMP, 
			MIN(STATUS) AS STATUS, MIN(ODS_JOBID) AS ODS_JOBID, 
			MIN(ODS_STATUS) AS ODS_STATUS, MIN(ODS_MSG) AS ODS_MSG, MIN(ODS_PDATE) AS ODS_PDATE, 
			MIN(ODS_SDATE) AS ODS_SDATE, MIN(ODS_EDATE) AS ODS_EDATE, MIN(ODS_PID) AS ODS_PID
	　FROM  (SELECT　A.SYS_NAME, A.EKORG, A.WORK_DATE, A.TABLE_NAME, A.GID, A.DG, A.GOD, B.FILE_NUMS, B.FILE_NUMS_COMP, 
					　B.FILE_ROWS, B.FILE_ROWS_COMP, A.STATUS, A.ODS_JOBID, A.ODS_STATUS, A.ODS_MSG, A.ODS_PDATE, 
					　A.ODS_SDATE, A.ODS_EDATE, A.ODS_PID
			　　FROM  (select * from dbo.STAGE_TAB_ACT union select * from dbo.STAGE_TAB_HIS) AS A 
			　　LEFT OUTER JOIN　dbo.V_FILE_INFO AS B ON A.SYS_NAME = B.SYS_NAME AND A.EKORG = B.EKORG AND 
								A.TABLE_NAME = B.TABLE_NAME AND A.WORK_DATE = B.WORK_DATE
				WHERE  (A.WORK_DATE IS NOT NULL)
			) AS A
	　GROUP BY DG, GID, WORK_DATE
) stg
left join ODSDB.dbo.ETL_EXECUTION_LOG ods on stg.DG = ods.DG and stg.GID = ods.GID and stg.WORK_DATE = ods.WORK_DATE
where stg.DG &lt;&gt; 'NULL' and stg.WORK_DATE = '2020-03-16'
order by isnull(stg.ODS_STATUS,0) desc, DG, GOD desc</DTS:VariableValue>
    </DTS:Variable>
    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{D0703AE1-B3B3-4FD2-BC04-54FBD03CBA7B}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;select sum(case when STAT = 'FAIL' then 1 else 0 end) as FAIL_AMT,&#xA;&#x9;   sum(case when STAT = 'SUCCESS' then 1 else 0 end) as SUCCESS_AMT,&#xA;&#x9;   sum(case when STAT = 'NONE' then 1 else 0 end) as NONE_AMT&#xA;from&#xA;(select case when stg.ODS_STATUS = 2 and isnull(ods.QUERY_ROW_COUNT,0) = isnull(ods.UPSERT_ROW_COUNT,0)&#xA;&#x9;&#x9;&#x9;then 'SUCCESS'&#xA;&#x9;&#x9;&#x9;when stg.ODS_STATUS = 3 or (stg.ODS_STATUS = 2 and isnull(ods.QUERY_ROW_COUNT,0) &lt;&gt; isnull(ods.UPSERT_ROW_COUNT,0))&#xA;&#x9;&#x9;&#x9;then 'FAIL'&#xA;&#x9;&#x9;&#x9;else 'NONE'&#xA;&#x9;&#x9;&#x9;end as STAT&#xA;from &#xA;(&quot; +  @[User::sqlFlow_Base] + &quot;)stat_tmp&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="sqlFlow_Stat">
      <DTS:VariableValue
        DTS:DataType="8">select sum(case when STAT = 'FAIL' then 1 else 0 end) as FAIL_AMT,
	   sum(case when STAT = 'SUCCESS' then 1 else 0 end) as SUCCESS_AMT,
	   sum(case when STAT = 'NONE' then 1 else 0 end) as NONE_AMT
from
(select case when stg.ODS_STATUS = 2 and isnull(ods.QUERY_ROW_COUNT,0) = isnull(ods.UPSERT_ROW_COUNT,0)
			then 'SUCCESS'
			when stg.ODS_STATUS = 3 or (stg.ODS_STATUS = 2 and isnull(ods.QUERY_ROW_COUNT,0) &lt;&gt; isnull(ods.UPSERT_ROW_COUNT,0))
			then 'FAIL'
			else 'NONE'
			end as STAT
from 
(SELECT DG, WORK_DATE, AVG(GOD) AS GOD, GID, COUNT(*) AS TAB_NUMS, 
			SUM(CASE WHEN FILE_NUMS = FILE_NUMS_COMP THEN 1 ELSE 0 END) AS TAB_NUMS_COMP, 
			SUM(FILE_NUMS) AS FILE_NUMS, SUM(FILE_NUMS_COMP) AS FILE_NUMS_COMP, 
			SUM(FILE_ROWS) AS FILE_ROWS, SUM(FILE_ROWS_COMP) AS FILE_ROWS_COMP, 
			MIN(STATUS) AS STATUS, MIN(ODS_JOBID) AS ODS_JOBID, 
			MIN(ODS_STATUS) AS ODS_STATUS, MIN(ODS_MSG) AS ODS_MSG, MIN(ODS_PDATE) AS ODS_PDATE, 
			MIN(ODS_SDATE) AS ODS_SDATE, MIN(ODS_EDATE) AS ODS_EDATE, MIN(ODS_PID) AS ODS_PID
	　FROM  (SELECT　A.SYS_NAME, A.EKORG, A.WORK_DATE, A.TABLE_NAME, A.GID, A.DG, A.GOD, B.FILE_NUMS, B.FILE_NUMS_COMP, 
					　B.FILE_ROWS, B.FILE_ROWS_COMP, A.STATUS, A.ODS_JOBID, A.ODS_STATUS, A.ODS_MSG, A.ODS_PDATE, 
					　A.ODS_SDATE, A.ODS_EDATE, A.ODS_PID
			　　FROM  (select * from dbo.STAGE_TAB_ACT union select * from dbo.STAGE_TAB_HIS) AS A 
			　　LEFT OUTER JOIN　dbo.V_FILE_INFO AS B ON A.SYS_NAME = B.SYS_NAME AND A.EKORG = B.EKORG AND 
								A.TABLE_NAME = B.TABLE_NAME AND A.WORK_DATE = B.WORK_DATE
				WHERE  (A.WORK_DATE IS NOT NULL)
			) AS A
	　GROUP BY DG, GID, WORK_DATE
) stg
left join ODSDB.dbo.ETL_EXECUTION_LOG ods on stg.DG = ods.DG and stg.GID = ods.GID and stg.WORK_DATE = ods.WORK_DATE
