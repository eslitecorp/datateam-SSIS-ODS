		/// <summary>
        /// This method is called when this script task executes in the control flow.
        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
            try
            {
                // 取得變數的value
                string tmpFlowFileName = Dts.Variables["User::tmpFlowFileName"].Value.ToString();
                string tmpWerksTransFileName = Dts.Variables["User::tmpWerksTransFileName"].Value.ToString();
                string templateExtFileName = Dts.Variables["User::tmpExtFileName"].Value.ToString();
                string templateFilePath = Dts.Variables["User::tmpFilePath"].Value.ToString(); 

                //timestamp
                string datetimeString = string.Format("{0:yyyyMMdd tthhmmss}", DateTime.Now);

                string rptFileNameFlow = Dts.Variables["User::rptFileName_Flow"].Value.ToString() + "_" + datetimeString + templateExtFileName;
                string rptFileNameWerksTrans = Dts.Variables["User::rptFileName_WerksTrans"].Value.ToString() + "_" + datetimeString  + templateExtFileName;

                // create empty EXCEL file: ODS 執行狀態統計
                string flowReportFile = System.IO.Path.Combine(templateFilePath, rptFileNameFlow); // empty full EXCEL file path
                Dts.Variables["User::rptFilePath_Flow"].Value = flowReportFile;
                System.IO.File.Copy(templateFilePath + tmpFlowFileName + templateExtFileName, flowReportFile, true);

                // create empty EXCEL file: 門市交易統計
                string werksTransReportFile = System.IO.Path.Combine(templateFilePath, rptFileNameWerksTrans);
                Dts.Variables["User::rptFilePath_WerksTrans"].Value = werksTransReportFile;
                System.IO.File.Copy(templateFilePath + tmpWerksTransFileName + templateExtFileName, werksTransReportFile, true);

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception e)
            {
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
