      <DTS:VariableValue
        DTS:DataType="8">SELECT * FROM (select main.WERKS , main.WERKS_Name , main.OPEN_DATE , main.CLOSE_DATE , main.TRANS_DATE , main.HYPOS_AMT , main.NEC_AMT from  (select werk.WERKS, werk.WERKS_Name,convert(varchar, OPEN_DATE, 111) as OPEN_DATE,convert(varchar, CLOSE_DATE, 111) as  CLOSE_DATE,convert(varchar,  werk.TRANS_DATE, 111) as TRANS_DATE, isnull(trans.HYPOS,0) as HYPOS_AMT, isnull(trans.NEC,0) as NEC_AMT
from 
(     select WERKS, WERKS_Name, trans_date, OPEN_DATE, CLOSE_DATE
		from [ODSDB].dbo.MF_DEPT_WERKS ,
	(select cast(DATEADD(DAY,-7,'2020-03-16') as date) as trans_date union  
	 select cast(DATEADD(DAY,-6,'2020-03-16') as date) as trans_date union
	 select cast(DATEADD(DAY,-5,'2020-03-16') as date) as trans_date union
	 select cast(DATEADD(DAY,-4,'2020-03-16') as date) as trans_date union
	 select cast(DATEADD(DAY,-3,'2020-03-16') as date) as trans_date union
	 select cast(DATEADD(DAY,-2,'2020-03-16') as date) as trans_date union
	 select cast(DATEADD(DAY,-1,'2020-03-16') as date) as trans_date) days_7
where ParentID_Mall not in ('999A','999B','9994','9996','9999','9998','#')
) werk
left join 
(select * from
	(
		select WERKS,cast(TRANS_DATE as date) as TRANS_DATE,count(*) as amt, 'HYPOS' as src_sys from ODSDB.dbo.TRANS_HEAD_HYPOS group by WERKS,cast(TRANS_DATE as date)
		union
		select WERKS,cast(TRANS_DATE as date) as TRANS_DATE,count(*) as amt, 'NEC' as src_sys from ODSDB.dbo.TRANS_HEAD_NEC group by WERKS,cast(TRANS_DATE as date)
	)tmp
	PIVOT (
		MAX(amt)
		FOR src_sys IN ([HYPOS], [NEC])
	) p
)trans
on  werk.trans_date = trans.TRANS_DATE and werk.WERKS = trans.WERKS
where OPEN_DATE is not null and dateadd(d,-7,cast('2020-03-16' as date)) between OPEN_DATE and CLOSE_DATE ) main
left  join (select werks , max(last_d) last_d from (select werks , max(etl_work_date) last_d from ODSDB.dbo.TRANS_HEAD_HYPOS group by werks
union 
select werks , max(etl_work_date) last_d from  ODSDB.dbo.TRANS_HEAD_NEC group by werks) lsd group by werks) lsd
on main.werks = lsd.werks
where lsd.last_d &gt;= dateadd(day , -7 ,'2020-03-16') 

UNION 

SELECT WERKS.WERKS , WERKS.WERKS_NAME , CONVERT(VARCHAR(10),WERKS.OPEN_DATE,23) OPEN_DATE , convert(varchar, WERKS.CLOSE_DATE, 111) as  CLOSE_DATE,
       convert(varchar,  WERKS.TRANS_DATE, 111) as TRANS_DATE, isnull(P.AMT,0) as HYPOS_AMT, isnull(P.AMT,0) as NEC_AMT
  FROM (SELECT WERKS.WERKS , WERKS.WERKS_NAME , WERKS.OPEN_DATE , WERKS.CLOSE_DATE , DAYS_7.TRANS_DATE
          FROM (SELECT WERKS.WERKS , WERKS.WERKS_NAME, WERKS.OPEN_DATE, WERKS.CLOSE_DATE
		          FROM ODSDB.DBO.MF_DEPT_WERKS WERKS 
		          JOIN (SELECT DISTINCT WERKS FROM ODSDB.dbo.TRANS_HEAD_EC ) EC_WERKS
		            ON WERKS.WERKS = EC_WERKS.WERKS ) WERKS,
	                   (SELECT CAST(DATEADD(DAY,-7,'2020-03-16') AS DATE) AS TRANS_DATE UNION  
	                    SELECT CAST(DATEADD(DAY,-6,'2020-03-16') AS DATE) AS TRANS_DATE UNION
	                    SELECT CAST(DATEADD(DAY,-5,'2020-03-16') AS DATE) AS TRANS_DATE UNION
	                    SELECT CAST(DATEADD(DAY,-4,'2020-03-16') AS DATE) AS TRANS_DATE UNION
	                    SELECT CAST(DATEADD(DAY,-3,'2020-03-16') AS DATE) AS TRANS_DATE UNION
	                    SELECT CAST(DATEADD(DAY,-2,'2020-03-16') AS DATE) AS TRANS_DATE UNION
	                    SELECT CAST(DATEADD(DAY,-1,'2020-03-16') AS DATE) AS TRANS_DATE) DAYS_7) WERKS
  LEFT JOIN (SELECT WERKS ,  CONVERT(VARCHAR(10), TRANS_DATE ,23) AS TRANS_DATE , COUNT(*) AMT FROM ODSDB.DBO.TRANS_HEAD_EC GROUP BY WERKS ,  CONVERT(VARCHAR(10), TRANS_DATE ,23)) P
    ON WERKS.TRANS_DATE = P.TRANS_DATE AND WERKS.WERKS = P.WERKS
  LEFT JOIN (SELECT WERKS , CONVERT(VARCHAR(10) , MAX(TRANS_DATE),23) LAST_D FROM ODSDB.DBO.TRANS_HEAD_EC GROUP BY WERKS) LAST_D
    ON WERKS.WERKS = LAST_D.WERKS
 WHERE WERKS.OPEN_DATE is not null and dateadd(d,-7,cast('2020-03-16' as date)) between OPEN_DATE and CLOSE_DATE
   AND LAST_D.LAST_D &gt;= DATEADD(DAY,-7 ,'2020-03-16') 

) AL
ORDER BY TRANS_DATE desc , WERKS </DTS:VariableValue>
