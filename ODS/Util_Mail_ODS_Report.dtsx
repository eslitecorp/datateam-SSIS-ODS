        public void Main()
        {
            String userParams = Dts.Variables["User::userParams"].Value.ToString();
            String[] paramsArr = userParams.Split(',');
            foreach (String var in paramsArr)
            {
                if (var.Split('$')[0] == "MailFrom")
                {
                    Dts.Variables["User::mailFromAddr"].Value = var.Split('$')[1];
                }
                else if (var.Split('$')[0] == "MailTo")
                {
                    Dts.Variables["User::mailToAddr"].Value = var.Split('$')[1];
