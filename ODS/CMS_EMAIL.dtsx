      , TMP.IaType
      , SS.Status  as STSTYPE
	  , '' as URL
	  , 0 as ClickCnt
  FROM 
    (
select Member_ID,Node_ID,Campaign_ID,min(Return_Time) as IsType_FIRST_TimeStamp,
case when SS.Status = 0 then 'EMAIL_OUTBOUND'
			when SS.Status in (1903,1907,1908,1909,1910,1914,1915,1916,1920,1922,1933,1934,1936,1939,101,102,104) then 'EMAIL_BOUNCE_SOFT'
			when SS.Status in (1000,1900,1901,1902,1904,1905,1906,1911,1912,1913,1917,1918,1919,1921,1923,1924,1925
				,1926,1927,1928,1929,1930,1931,1932,1935,1937,1938,1940,1941,103,105,106,107) then 'EMAIL_BOUNCE_SOFT'
			end as IaType
  from OMI.[EmailStatus] SS
 group by Member_ID,Node_ID,Campaign_ID,
 case when SS.Status = 0 then 'EMAIL_OUTBOUND'
			when SS.Status in (1903,1907,1908,1909,1910,1914,1915,1916,1920,1922,1933,1934,1936,1939,101,102,104) then 'EMAIL_BOUNCE_SOFT'
			when SS.Status in (1000,1900,1901,1902,1904,1905,1906,1911,1912,1913,1917,1918,1919,1921,1923,1924,1925
				,1926,1927,1928,1929,1930,1931,1932,1935,1937,1938,1940,1941,103,105,106,107) then 'EMAIL_BOUNCE_SOFT'
			end 
    )TMP
  join  [StagingDB_TW].[OMI].[EmailStatus] SS
  on SS.Campaign_ID = TMP.Campaign_ID
  and SS.Member_ID = TMP.Member_ID
  and SS.Node_ID = TMP.Node_ID
  and SS.Return_Time = TMP.IsType_FIRST_TimeStamp
  and (
   case when SS.Status = 0 then 'EMAIL_OUTBOUND'
			when SS.Status in (1903,1907,1908,1909,1910,1914,1915,1916,1920,1922,1933,1934,1936,1939,101,102,104) then 'EMAIL_BOUNCE_SOFT'
			when SS.Status in (1000,1900,1901,1902,1904,1905,1906,1911,1912,1913,1917,1918,1919,1921,1923,1924,1925
