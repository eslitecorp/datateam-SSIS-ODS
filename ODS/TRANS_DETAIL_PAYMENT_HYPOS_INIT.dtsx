                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="要執行的 SQL 命令。"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select head.PSN
     , head.STORE_NO
	 , head.BUSINESS_DATE
	 , head.TM_NO
	 , head.TRN_SEQ
	 , pay.ITEM_SEQ
, head.TM_DATE -- ETL組合TRANS_DATE
	 , head.TM_TIME -- ETL組合TRANS_DATE
	 , case when concat(head.LEAD_CODE,head.TRN_TYPE) in ('T211','T511') THEN '1' ELSE head.FLAG END AS FLAG -- 正/負向交易
	 , pay.PAY_ID
	 , pay.PAY_KIND
	 , pay.PAY_AMT
	 , case when PAY_KIND = '05' 
			then (select top 1 BankNo from POSNEC.CREDITCARDNO where CardNo = left(MEMO2,6) order by CreateDate)
			else NULL END AS BANK_NO      -- 信用卡所屬銀行代碼
	 , case when PAY_KIND = '05' 
			then (select top 1 BankNm from POSNEC.CREDITCARDNO where CardNo = left(MEMO2,6) order by CreateDate)
			else NULL END AS BANK_NAME    -- 信用卡所屬銀行名稱
from (select * from POS.TRN_HEAD where BUSINESS_DATE between replace(?,'-','') and replace(EOMONTH(?),'-','')) head
left join POS.TRN_PAYMENT pay on head.PSN = pay.PSN
where pay.PSN between 
	  (select min(PSN) from POS.TRN_HEAD where BUSINESS_DATE between replace(?,'-','') and replace(EOMONTH(?),'-','')) 
	  and 
	  (select max(PSN) from POS.TRN_HEAD where BUSINESS_DATE between replace(?,'-','') and replace(EOMONTH(?),'-','')) 
and head.VOID_FLAG &lt;&gt; 'V'
order by head.STORE_NO 
     , head.BUSINESS_DATE
	 , head.TM_NO 
	 , head.TRN_SEQ
	 , pay.ITEM_SEQ</property>
