	, CO.c_wine as MEM_WINE
	, CO.c_mobile_p as MEM_MOBILE_COUNTRY
FROM [StagingDB_TW].[LMS].[CUST_OUT] CO
outer apply (
	select top 1 *
	  from [StagingDB_TW].[LMS].[CUST_CRD_CAL] CCO 
